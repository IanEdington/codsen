{
  "name": "email-homey",
  "version": "2.1.0",
  "description": "Generate homepage in the Browsersync root with links/screenshots to all your email templates",
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/codsen/email-homey.git"
  },
  "homepage": "https://bitbucket.org/codsen/email-homey#readme",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "bin": {
    "homey": "cli.js"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "format": "prettier '*.{js,ts,css,less,scss,vue,gql,md}' --write && ./node_modules/.bin/eslint cli.js test/*.js --fix",
    "precommit": "npm run format && npm test",
    "test": "./node_modules/.bin/eslint cli.js && nyc --reporter=html --reporter=text ava"
  },
  "ava": {
    "compileEnhancements": false,
    "require": [
      "esm"
    ],
    "verbose": true
  },
  "clinton": {
    "rules": {
      "xo": 0
    }
  },
  "esm": {
    "await": true,
    "cjs": true
  },
  "keywords": [
    "email",
    "templates",
    "gulp",
    "gulpplugin",
    "root",
    "folder",
    "build",
    "system",
    "generate",
    "home",
    "page",
    "homepage",
    "browsersync"
  ],
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "contributors": true,
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "contribution_types": [
      "Blogposts",
      "Bug reports",
      "Code",
      "Design",
      "Documentation",
      "Event Organizing",
      "Examples",
      "Financial",
      "Funding Finding",
      "Ideas, Planning, & Feedback",
      "Infrastructure (Hosting, Build-Tools, etc)",
      "Plugin/utility libraries",
      "Answering Questions",
      "Reviewed Pull Requests",
      "Talks",
      "Tests",
      "Tools",
      "Translation",
      "Tutorials",
      "Videos"
    ],
    "contributors": [
      {
        "contribution": [
          "Code",
          "Documentation",
          "Tests"
        ],
        "username": "revelt"
      }
    ],
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": []
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      },
      "devDependencies": [],
      "travisVersionsOverride": []
    }
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "globby": "^8.0.1",
    "lodash.uniq": "^4.5.0",
    "log-symbols": "^2.2.0",
    "meow": "^5.0.0",
    "string-splice": "^1.3.0",
    "update-notifier": "^2.5.0"
  },
  "devDependencies": {
    "ava": "latest",
    "codacy-coverage": "latest",
    "coveralls": "latest",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-ava": "^4.5.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-no-unsanitized": "latest",
    "eslint-plugin-no-wildcard-postmessage": "latest",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-scanjs-rules": "latest",
    "esm": "latest",
    "execa": "^0.10.0",
    "husky": "latest",
    "nyc": "latest",
    "prettier": "latest"
  }
}
