{
  "{%": {
    "type": "opening",
    "sibling": ["%}", "-%}"]
  },
  "%}": {
    "type": "closing",
    "sibling": ["{%", "{%-"]
  },
  "{%-": {
    "type": "opening",
    "sibling": ["-%}", "%}"]
  },
  "-%}": {
    "type": "closing",
    "sibling": ["{%-", "{%"]
  },
  "*|": {
    "type": "opening",
    "sibling": "|*"
  },
  "|*": {
    "type": "closing",
    "sibling": "*|"
  },
  "$": {
    "type": "opening",
    "sibling": "$"
  },
  "$": {
    "type": "closing",
    "sibling": "$"
  }
}
