{
  "name": "emlint",
  "version": "2.18.10",
  "description": "Pluggable email template code linter",
  "license": "MIT",
  "repository": "https://gitlab.com/codsen/codsen/",
  "homepage": "https://gitlab.com/codsen/codsen/tree/master/packages/emlint",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "keywords": [
    "analysis",
    "automatic",
    "automatically",
    "broken",
    "check",
    "code",
    "css",
    "development",
    "email",
    "errors",
    "fix",
    "html",
    "lint",
    "linter",
    "linting",
    "static",
    "template",
    "tool",
    "utility",
    "xhtml"
  ],
  "bin": {
    "emlint": "bin/cli.js"
  },
  "main": "dist/emlint.cjs.js",
  "module": "dist/emlint.esm.js",
  "browser": "dist/emlint.umd.js",
  "scripts": {
    "build": "node bin/helper && rollup -c",
    "dev": "node bin/helper && rollup -c --dev",
    "devunittest": "npm run dev && npm run tap -- \"test/rules/bad-html-entity/bad-named-html-entity-unrecognised.js\" --only",
    "devunittest2": "npm run dev && ./node_modules/.bin/tap --only",
    "format": "npm run lect && npm run prettier && npm run lint",
    "lect": "lect",
    "lint": "./node_modules/.bin/eslint \"**/*.js\" --fix --ignore-pattern \"dist/*\" --ignore-pattern \"rollup.config.js\" --ignore-pattern \"tap/**\"",
    "perf": "node perf/check",
    "prepare": "npm run build",
    "prettier": "../../node_modules/prettier/bin-prettier.js '*.{js,css,scss,vue,md}' --write",
    "republish": "npm publish || :",
    "tap": "tap",
    "pretest": "npm run build",
    "test": "npm run lint && npm run unittest && npm run format",
    "unittest": "./node_modules/.bin/tap --no-only -R 'terse' -o testStats.md && npm run perf",
    "version": "npm run build && git add ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm test"
    }
  },
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": [
        ""
      ]
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "req": "{ Linter, version }",
    "special": true,
    "various": {
      "devDependencies": [
        "ast-deep-contains",
        "browser-sync",
        "ranges-apply",
        "rollup-plugin-node-builtins",
        "rollup-plugin-node-globals"
      ],
      "travisVersionsOverride": []
    }
  },
  "tap": {
    "coverage-report": [
      "json-summary",
      "text"
    ],
    "nyc-arg": [
      "--exclude=**/*.cjs.js",
      "--exclude=**/*.umd.js"
    ],
    "timeout": 0
  },
  "dependencies": {
    "ast-monkey-traverse": "^1.12.15",
    "ast-monkey-util": "^1.1.8",
    "codsen-parser": "^0.7.2",
    "define-lazy-prop": "^2.0.0",
    "globby": "^11.0.1",
    "he": "^1.2.0",
    "html-all-known-attributes": "^2.0.5",
    "html-entities-not-email-friendly": "^0.2.6",
    "is-d": "^1.0.0",
    "is-language-code": "^1.0.9",
    "is-media-descriptor": "^1.2.15",
    "is-relative-uri": "^1.0.16",
    "is-url-superb": "^4.0.0",
    "leven": "^3.1.0",
    "line-column": "^1.0.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isregexp": "^4.0.1",
    "matcher": "^3.0.0",
    "meow": "^7.1.0",
    "mime-db": "^1.44.0",
    "object-path": "^0.11.4",
    "p-reduce": "^2.1.0",
    "rgba-regex": "^1.0.0",
    "string-find-malformed": "^1.1.11",
    "string-fix-broken-named-entities": "^3.0.5",
    "string-left-right": "^2.3.26",
    "string-match-left-right": "^4.0.10",
    "string-process-comma-separated": "^1.2.10",
    "update-notifier": "^4.1.1",
    "url-regex": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-strip": "^2.0.0",
    "ast-deep-contains": "^1.1.16",
    "benchmark": "^2.1.4",
    "eslint": "^7.7.0",
    "lect": "^0.13.9",
    "prettier": "^2.0.5",
    "ranges-apply": "^3.1.12",
    "rollup": "^2.26.4",
    "rollup-plugin-ascii": "^0.0.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-terser": "^7.0.0",
    "tap": "^14.10.8"
  }
}
