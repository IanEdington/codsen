{
  "name": "email-comb",
  "version": "1.0.1",
  "description": "Remove unused CSS from email templates",
  "license": "MIT",
  "engines": {
    "node": ">=6",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/codsen/email-comb.git"
  },
  "homepage": "https://bitbucket.org/codsen/email-comb/",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "https://codsen.com"
  },
  "main": "dist/email-comb.cjs.js",
  "module": "dist/email-comb.esm.js",
  "browser": "dist/email-comb.umd.js",
  "scripts": {
    "build": "rollup -c",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "rollup -c --dev --silent",
    "format": "./node_modules/.bin/eslint src/*.js test/*.js --fix",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/eslint src/*.js test/*.js && nyc ava",
    "version": "npm run build && git add ."
  },
  "ava": {
    "cache": false,
    "compileEnhancements": false,
    "require": [
      "esm"
    ],
    "timeout": "1m",
    "verbose": true
  },
  "clinton": {
    "rules": {
      "xo": 0
    }
  },
  "esm": {
    "await": true,
    "cjs": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm test"
    }
  },
  "keywords": [
    "email",
    "remove",
    "unused",
    "css",
    "head",
    "styles",
    "body",
    "uncss",
    "inline",
    "from"
  ],
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "contributors": true,
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "contribution_types": [
      "Blogposts",
      "Bug reports",
      "Code",
      "Design",
      "Documentation",
      "Event Organizing",
      "Examples",
      "Financial",
      "Funding Finding",
      "Ideas, Planning, & Feedback",
      "Infrastructure (Hosting, Build-Tools, etc)",
      "Plugin/utility libraries",
      "Answering Questions",
      "Reviewed Pull Requests",
      "Talks",
      "Tests",
      "Tools",
      "Translation",
      "Tutorials",
      "Videos"
    ],
    "contributors": [
      {
        "contribution": [
          "Code",
          "Documentation",
          "Tests"
        ],
        "username": "revelt"
      },
      {
        "contribution": [
          "Bug reports"
        ],
        "username": "stevenvachon"
      },
      {
        "contribution": [
          "Bug reports"
        ],
        "username": "hellocosmin"
      }
    ],
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": [
        "",
        "Adapted a `generateShortname()` function from:\nhttps://github.com/cazzer/gulp-selectors/blob/master/lib/utils/generate-shortname.js\nMIT License (MIT) Copyright Â© 2014 Caleb Brewer"
      ]
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      },
      "devDependencies": [
        "rollup-plugin-node-builtins"
      ],
      "travisVersionsOverride": []
    }
  },
  "nyc": {
    "instrument": false,
    "require": [
      "@babel/register"
    ],
    "sourceMap": false
  },
  "dependencies": {
    "array-pull-all-with-glob": "^4.7.0",
    "ast-is-empty": "^1.3.1",
    "lodash.intersection": "^4.4.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.pullall": "^4.2.0",
    "lodash.uniq": "^4.5.0",
    "matcher": "^1.1.1",
    "ranges-apply": "^2.8.1",
    "ranges-push": "^2.12.0",
    "regex-empty-conditional-comments": "^1.2.1",
    "string-extract-class-names": "^5.1.0",
    "string-match-left-right": "^3.5.0",
    "string-range-expander": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "ava": "^1.0.1",
    "babel-plugin-istanbul": "^5.1.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.11.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-ava": "^5.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "eslint-plugin-no-wildcard-postmessage": "^0.1.3",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "husky": "^1.2.1",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3",
    "rollup": "^0.68.2",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-license": "^0.7.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-strip": "^1.2.0",
    "rollup-plugin-terser": "^3.0.0"
  }
}
