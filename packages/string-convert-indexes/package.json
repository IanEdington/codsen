{
  "name": "string-convert-indexes",
  "version": "1.0.1",
  "description": "Convert string character indexes from JS native index-based to Unicode character-count-based and backwards.",
  "license": "MIT",
  "engines": {
    "node": ">=6",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codsen/string-convert-indexes.git"
  },
  "bugs": {
    "url": "https://github.com/codsen/string-convert-indexes/issues"
  },
  "homepage": "https://github.com/codsen/string-convert-indexes#readme",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "main": "dist/string-convert-indexes.cjs.js",
  "module": "dist/string-convert-indexes.esm.js",
  "browser": "dist/string-convert-indexes.umd.js",
  "scripts": {
    "build": "rollup -c",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "rollup -c -w",
    "precommit": "npm test",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/eslint src/*.js test/*.js && nyc --reporter=html --reporter=text ava"
  },
  "ava": {
    "verbose": true
  },
  "clinton": {
    "rules": {
      "xo": 0
    }
  },
  "keywords": [
    "string",
    "indexes",
    "character",
    "unicode",
    "based",
    "convert",
    "count",
    "emoji",
    "astral"
  ],
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "contributors": true,
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "contribution_types": [
      "Answering Questions",
      "Blogposts",
      "Bug reports*",
      "Code*",
      "Documentation*",
      "Design",
      "Examples",
      "Event Organizers",
      "Financial Support",
      "Funding/Grant Finders",
      "Ideas & Planning",
      "Infrastructure (Hosting, Build-Tools, etc)",
      "Plugin/utility libraries",
      "Reviewed Pull Requests",
      "Tools",
      "Translation",
      "Tests*",
      "Tutorials",
      "Talks",
      "Videos"
    ],
    "contributors": [
      {
        "contribution": [
          "Code",
          "Documentation",
          "Tests"
        ],
        "username": "revelt"
      }
    ],
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": ""
    },
    "licence": {
      "extras": []
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      }
    }
  },
  "dependencies": {
    "ast-monkey": "^7.2.5",
    "check-types-mini": "^3.0.2",
    "is-natural-number": "*",
    "is-natural-number-string": "*",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isplainobject": "^4.0.6",
    "ordinal-number-suffix": "^0.1.1"
  },
  "devDependencies": {
    "ava": "*",
    "babel-preset-env": "^1.6.1",
    "coveralls": "*",
    "eslint": "^4.14.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-ava": "^4.4.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "*",
    "nyc": "*",
    "rollup": "^0.53.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "uglify-es": "^3.3.1"
  }
}
