{
  "name": "ast-monkey",
  "version": "7.3.2",
  "description": "Utility library for ops on parsed HTML (AST's) or anything nested (plain objects within arrays within plain objects)",
  "license": "MIT",
  "engines": {
    "node": ">=6",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codsen/ast-monkey.git"
  },
  "bugs": {
    "url": "https://github.com/codsen/ast-monkey/issues"
  },
  "homepage": "https://github.com/codsen/ast-monkey#readme",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "main": "dist/ast-monkey.cjs.js",
  "module": "dist/ast-monkey.esm.js",
  "browser": "dist/ast-monkey.umd.js",
  "scripts": {
    "build": "rollup -c",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "rollup -c -w",
    "precommit": "npm test",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/eslint src/*.js test/*.js && nyc --reporter=html --reporter=text ava"
  },
  "ava": {
    "verbose": true
  },
  "clinton": {
    "rules": {
      "xo": 0
    }
  },
  "keywords": [
    "ast",
    "by",
    "delete",
    "drop",
    "from",
    "html",
    "key",
    "object",
    "plain",
    "parser",
    "posthtml",
    "posthtml-tree",
    "posthtmltree",
    "tree",
    "xml",
    "value"
  ],
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "contributors": true,
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "contribution_types": [
      "Answering Questions",
      "Blogposts",
      "Bug reports*",
      "Code*",
      "Design",
      "Documentation*",
      "Event Organizers",
      "Examples",
      "Financial Support",
      "Funding/Grant Finders",
      "Ideas & Planning",
      "Infrastructure (Hosting, Build-Tools, etc)",
      "Plugin/utility libraries",
      "Reviewed Pull Requests",
      "Talks",
      "Tests*",
      "Tools",
      "Translation",
      "Tutorials",
      "Videos"
    ],
    "contributors": [
      {
        "contribution": [
          "Code",
          "Documentation",
          "Tests"
        ],
        "username": "revelt"
      }
    ],
    "eslintrc": {
      "add": [
        "whatnot"
      ],
      "remove": []
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": ""
    },
    "licence": {
      "extras": []
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      }
    }
  },
  "dependencies": {
    "ast-compare": "^1.1.6",
    "ast-monkey-traverse": "^1.1.3",
    "check-types-mini": "^3.1.0",
    "is-natural-number": "^4.0.1",
    "lodash.clonedeep": "*",
    "type-detect": "^4.0.8",
    "util-array-object-or-both": "^2.0.3"
  },
  "devDependencies": {
    "ava": "*",
    "babel-preset-env": "^1.6.1",
    "coveralls": "*",
    "eslint": "^4.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-ava": "^4.5.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "*",
    "nyc": "*",
    "rollup": "^0.55.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-uglify": "^3.0.0",
    "uglify-es": "^3.3.9"
  }
}
