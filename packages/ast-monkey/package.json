{
  "name": "ast-monkey",
  "version": "6.3.0",
  "description": "Utility library for operations on parsed HTML (AST's)",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test && doctoc readme.md",
    "test": "clear && standard && nyc --reporter=html --reporter=text ava",
    "watch": "nodemon --quiet --watch . --exec npm run test"
  },
  "repository": "codsen/ast-monkey",
  "keywords": [
    "ast",
    "by",
    "delete",
    "drop",
    "from",
    "html",
    "key",
    "object",
    "plain",
    "parser",
    "posthtml",
    "posthtml-tree",
    "posthtmltree",
    "tree",
    "xml",
    "value"
  ],
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/ast-monkey/issues"
  },
  "homepage": "https://github.com/codsen/ast-monkey#readme",
  "dependencies": {
    "check-types-mini": "^2.3.0",
    "is-natural-number": "^4.0.1",
    "lodash.clonedeep": "*",
    "lodash.isequal": "*",
    "lodash.isplainobject": "*",
    "util-array-object-or-both": "^1.0.8"
  },
  "devDependencies": {
    "ava": "*",
    "clear-cli": "*",
    "coveralls": "*",
    "husky": "^0.14.3",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.3",
    "standard": "^10.0.2"
  },
  "ava": {
    "verbose": true
  },
  "engines": {
    "node": ">=5.11.0"
  }
}
