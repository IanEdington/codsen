{
  "name": "ast-monkey",
  "version": "6.3.2",
  "description": "Utility library for operations on parsed HTML (AST's)",
  "main": "index-es5.js",
  "scripts": {
    "build": "babel index.js --presets babel-preset-es2015 --out-file index-es5.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "prepare": "npm run build",
    "test": "clear && ./node_modules/.bin/eslint index.js test.js && nyc --reporter=html --reporter=text ava && npm run build",
    "watch": "nodemon --quiet --watch . --exec npm run test"
  },
  "repository": "codsen/ast-monkey",
  "keywords": [
    "ast",
    "by",
    "delete",
    "drop",
    "from",
    "html",
    "key",
    "object",
    "plain",
    "parser",
    "posthtml",
    "posthtml-tree",
    "posthtmltree",
    "tree",
    "xml",
    "value"
  ],
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/ast-monkey/issues"
  },
  "homepage": "https://github.com/codsen/ast-monkey#readme",
  "dependencies": {
    "check-types-mini": "^2.6.0",
    "is-natural-number": "^4.0.1",
    "lodash.clonedeep": "*",
    "lodash.isequal": "*",
    "lodash.isplainobject": "*",
    "util-array-object-or-both": "^1.1.2"
  },
  "devDependencies": {
    "ava": "*",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clear-cli": "*",
    "coveralls": "*",
    "eslint": "^4.7.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "husky": "*",
    "nyc": "*",
    "nodemon": "*"
  },
  "ava": {
    "verbose": true
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
