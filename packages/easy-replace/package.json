{
  "name": "easy-replace",
  "version": "2.8.1",
  "description": "Replace strings with optional lookarounds, but without regexes",
  "main": "er.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test && doctoc README.md",
    "test": "standard && nyc --reporter=html --reporter=text ava"
  },
  "repository": "codsen/easy-replace",
  "keywords": [
    "search",
    "negative",
    "positive",
    "lookbehind",
    "lookahead",
    "string",
    "lookaround",
    "easy",
    "replace",
    "replacement",
    "find"
  ],
  "author": {
    "name": "Roy Reveltas",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/easy-replace/issues"
  },
  "homepage": "https://github.com/codsen/easy-replace#readme",
  "dependencies": {
    "check-types-mini": "^2.2.1",
    "lodash.isnumber": "*",
    "lodash.toarray": "*",
    "lodash.without": "*"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "coveralls": "*",
    "doctoc": "*",
    "husky": "^0.14.3",
    "nyc": "^11.0.3",
    "standard": "^10.0.2"
  },
  "ava": {
    "verbose": false
  }
}
