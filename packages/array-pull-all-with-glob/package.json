{
  "name": "array-pull-all-with-glob",
  "version": "3.0.2",
  "description": "pullAllWithGlob - like _.pullAll but pulling stronger, with globs",
  "main": "index.js",
  "scripts": {
    "build": "babel index.js --presets babel-preset-es2015 --out-dir es5",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "prepublish": "npm run build",
    "test": "standard && nyc --reporter=html --reporter=text ava"
  },
  "repository": "codsen/array-pull-all-with-glob",
  "keywords": [
    "array",
    "delete",
    "pull",
    "pullall",
    "remove",
    "pulall",
    "pullal",
    "pulal",
    "element",
    "from",
    "glob",
    "pattern",
    "whitelist"
  ],
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/array-pull-all-with-glob/issues"
  },
  "homepage": "https://github.com/codsen/array-pull-all-with-glob#readme",
  "dependencies": {
    "lodash.pullall": "*",
    "matcher": "^1.0.0"
  },
  "devDependencies": {
    "ava": "*",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "*",
    "husky": "*",
    "nyc": "*",
    "standard": "^10.0.3"
  },
  "ava": {
    "verbose": true
  },
  "standard": {
    "ignore": [
      "es5/"
    ]
  }
