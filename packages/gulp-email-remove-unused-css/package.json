{
  "name": "gulp-email-remove-unused-css",
  "version": "3.4.0",
  "description": "Gulp plugin to remove unused CSS classes/id's from styles in HTML HEAD and inline within BODY",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "repository": "https://bitbucket.org/codsen/codsen/src/master/packages/gulp-email-remove-unused-css",
  "homepage": "https://bitbucket.org/codsen/codsen/src/master/packages/gulp-email-remove-unused-css",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "main": "index-es5.js",
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "coverage": "nyc report --reporter=json-summary",
    "dev": "rm -rf dist && rollup -c --dev --silent",
    "format": "prettier '*.{js,ts,css,less,scss,vue,gql,md}' --write && npm run lint",
    "lint": "./node_modules/.bin/eslint \"**/*.js\" --fix --ignore-pattern \"dist/*\" --ignore-pattern \"rollup.config.js\"",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "npm run lint && npm run unittest",
    "unittest": "./node_modules/.bin/nyc ava && npm run coverage",
    "version": "npm run build && git add ."
  },
  "ava": {
    "compileEnhancements": false,
    "require": [
      "esm"
    ],
    "verbose": true
  },
  "esm": {
    "await": true,
    "cjs": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm test"
    }
  },
  "keywords": [
    "gulp",
    "gulpplugin",
    "email",
    "remove",
    "unused",
    "css",
    "head",
    "styles",
    "body",
    "uncss",
    "inline",
    "from"
  ],
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "contributors": true,
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": false,
      "travis": true,
      "vulnerabilities": true
    },
    "contribution_types": [
      "Blogposts",
      "Bug reports",
      "Code",
      "Design",
      "Documentation",
      "Event Organizing",
      "Examples",
      "Financial",
      "Funding Finding",
      "Ideas, Planning, & Feedback",
      "Infrastructure (Hosting, Build-Tools, etc)",
      "Plugin/utility libraries",
      "Answering Questions",
      "Reviewed Pull Requests",
      "Talks",
      "Tests",
      "Tools",
      "Translation",
      "Tutorials",
      "Videos"
    ],
    "contributors": [
      {
        "contribution": [
          "Code",
          "Documentation",
          "Tests"
        ],
        "username": "revelt"
      }
    ],
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": [
        ""
      ]
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      },
      "devDependencies": [],
      "travisVersionsOverride": []
    }
  },
  "nyc": {
    "instrument": false,
    "require": [
      "@babel/register"
    ],
    "sourceMap": false
  },
  "dependencies": {
    "email-remove-unused-css": "^4.7.0",
    "gulp-util": "^3.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "babel-plugin-istanbul": "^5.1.0",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-ava": "^5.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "eslint-plugin-no-wildcard-postmessage": "^0.1.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "esm": "^3.0.84",
    "prettier": "^1.15.3"
  }
}
