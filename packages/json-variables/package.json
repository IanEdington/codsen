{
  "name": "json-variables",
  "version": "4.5.2",
  "description": "Postprocessor for JSON to allow keys referencing keys",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test && doctoc readme.md",
    "test": "standard && nyc --reporter=html --reporter=text ava",
    "watch": "nodemon --quiet --watch . --exec npm run test"
  },
  "repository": "codsen/json-variables",
  "keywords": [
    "json",
    "normalize",
    "normalise",
    "schema",
    "files",
    "fill",
    "keyset",
    "keys",
    "same",
    "comb",
    "variable",
    "variables",
    "preprocessor",
    "postprocessor",
    "processor",
    "process"
  ],
  "author": {
    "name": "Roy Reveltas",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/json-variables/issues"
  },
  "homepage": "https://github.com/codsen/json-variables#readme",
  "dependencies": {
    "arrayiffy-if-string": "*",
    "ast-monkey": "^5.4.1",
    "check-types-mini": "^2.0.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.includes": "^4.3.0",
    "lodash.trim": "^4.5.1",
    "matcher": "^1.0.0",
    "num-sort": "^1.0.0",
    "object-assign": "^4.1.1",
    "object-path": "^0.11.4",
    "splice-string": "^2.0.0",
    "str-indexes-of-plus": "^1.0.2",
    "string-length": "^1.0.1",
    "string-slice": "^0.1.0",
    "type-detect": "^4.0.0"
  },
  "devDependencies": {
    "ava": "*",
    "coveralls": "*",
    "husky": "^0.13.4",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.2",
    "standard": "^10.0.0"
  },
  "ava": {
    "verbose": false
  },
  "engines": {
    "node": ">=5.11.0"
  }
}
