{
  "name": "json-variables",
  "version": "4.6.0",
  "description": "Postprocessor for JSON to allow keys referencing keys",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "test": "clear && standard && nyc --reporter=html --reporter=text ava",
    "watch": "nodemon --quiet --watch . --exec npm run test"
  },
  "repository": "codsen/json-variables",
  "keywords": [
    "json",
    "normalize",
    "normalise",
    "schema",
    "files",
    "fill",
    "keyset",
    "keys",
    "same",
    "comb",
    "variable",
    "variables",
    "preprocessor",
    "postprocessor",
    "processor",
    "process"
  ],
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/json-variables/issues"
  },
  "homepage": "https://github.com/codsen/json-variables#readme",
  "dependencies": {
    "arrayiffy-if-string": "*",
    "ast-monkey": "^6.3.2",
    "check-types-mini": "^2.4.1",
    "lodash.clonedeep": "*",
    "lodash.includes": "*",
    "lodash.trim": "*",
    "matcher": "^1.0.0",
    "num-sort": "^1.0.0",
    "object-path": "^0.11.4",
    "splice-string": "^2.0.0",
    "str-indexes-of-plus": "^1.0.2",
    "string-length": "^2.0.0",
    "string-slice": "^0.1.0",
    "type-detect": "^4.0.3"
  },
  "devDependencies": {
    "ava": "*",
    "clear-cli": "*",
    "coveralls": "*",
    "doctoc": "*",
    "husky": "*",
    "nodemon": "*",
    "nyc": "^11.2.1",
    "standard": "^10.0.3"
  },
  "ava": {
    "verbose": false
  },
  "engines": {
    "node": ">=5.11.0"
  }
}
