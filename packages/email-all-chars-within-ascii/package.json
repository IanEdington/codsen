{
  "name": "email-all-chars-within-ascii",
  "version": "1.2.0",
  "description": "Scans all characters within a string and checks are they within ASCII range",
  "main": "index-es5.js",
  "scripts": {
    "build": "babel index.js --presets babel-preset-es2015 --out-file index-es5.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "prepare": "npm run build",
    "test": "clear && ./node_modules/.bin/eslint index.js test.js && nyc --reporter=html --reporter=text ava && npm run build"
  },
  "repository": "codsen/email-all-chars-within-ascii",
  "keywords": [
    "email",
    "templates",
    "template",
    "characters",
    "ascii",
    "range",
    "encoded",
    "7bit",
    "validate"
  ],
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/email-all-chars-within-ascii/issues"
  },
  "homepage": "https://github.com/codsen/email-all-chars-within-ascii#readme",
  "dependencies": {
    "check-types-mini": "^2.6.0",
    "lodash.isplainobject": "*"
  },
  "devDependencies": {
    "ava": "*",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clear-cli": "*",
    "coveralls": "*",
    "eslint": "^4.7.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "husky": "*",
    "nyc": "*"
  },
  "ava": {
    "verbose": true
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
