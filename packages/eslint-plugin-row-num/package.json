{
  "name": "eslint-plugin-row-num",
  "version": "1.1.0",
  "description": "updates row numbers in front of each encountered console.log",
  "license": "MIT",
  "engines": {
    "node": ">=8.9"
  },
  "repository": "https://gitlab.com/codsen/codsen/",
  "homepage": "https://gitlab.com/codsen/codsen/tree/master/packages/eslint-plugin-row-num",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "keywords": [
    "console",
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "js-row-num",
    "log",
    "num",
    "numbers",
    "row",
    "row-num",
    "update"
  ],
  "main": "dist/eslint-plugin-row-num.cjs.js",
  "scripts": {
    "dev": "echo\"\"",
    "devunittest": "npm run test",
    "format": "npm run lect && npm run prettier && npm run lint",
    "lect": "lect",
    "lint": "../../node_modules/eslint/bin/eslint.js \"**/*.js\" --fix --config \"../../.eslintrc.json\"",
    "prettier": "../../node_modules/prettier/bin-prettier.js '*.{js,css,scss,vue,md}' --write",
    "test": "npm run lint && npm run unittest && npm run format",
    "unittest": "mocha --recursively",
    "version": "git add ."
  },
  "lect": {
    "badges": {
      "cov": false,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": false,
      "travis": false,
      "vulnerabilities": false
    },
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": [
        ""
      ]
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "special": true,
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      },
      "devDependencies": [
        "eslint",
        "mocha"
      ]
    }
  },
  "tap": {
    "coverage-report": "json-summary",
    "timeout": 0
  },
  "dependencies": {
    "import-modules": "^2.0.0",
    "js-row-num": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.5",
    "benchmark": "^2.1.4",
    "eslint": "^6.7.2",
    "js-row-num-cli": "^1.6.10",
    "lect": "^0.11.3",
    "mocha": "^6.2.2",
    "rollup": "^1.27.9",
    "rollup-plugin-ascii": "^0.0.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-strip": "^1.2.2",
    "rollup-plugin-terser": "^5.1.2",
    "tap": "^14.10.2"
  }
}
