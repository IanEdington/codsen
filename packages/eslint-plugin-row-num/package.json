{
  "name": "eslint-plugin-row-num",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=8.9"
  },
  "description": "updates row numbers in front of each encountered console.log",
  "repository": "https://gitlab.com/codsen/codsen/",
  "homepage": "https://gitlab.com/codsen/codsen/tree/master/packages/eslint-plugin-row-num",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "keywords": [
    "row",
    "num",
    "numbers",
    "console",
    "log",
    "update",
    "row-num",
    "js-row-num",
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "main": "lib/index.js",
  "scripts": {
    "format": "npm run lect && npm run prettier && npm run lint",
    "dev": "echo\"\"",
    "lint": "./node_modules/.bin/eslint **/*.js --fix",
    "lect": "lect",
    "prettier": "../../node_modules/prettier/bin-prettier.js '*.{js,css,scss,vue,md}' --write",
    "test": "mocha test --recursive",
    "unittest": "npm run test",
    "version": "npm run build && git add ."
  },
  "lect": {
    "special": true
  },
  "dependencies": {
    "import-modules": "^2.0.0",
    "js-row-num": "^2.5.0"
  },
  "devDependencies": {
    "eslint": "^6.7.2",
    "lect": "^0.11.2",
    "mocha": "^6.2.2"
  }
