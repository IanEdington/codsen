{
  "name": "ava",
  "version": "2.2.0",
  "description": "Testing can be a drag. AVA helps you get it done.",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=8.9.4 <9 || >=10.0.0 <11 || >=12.0.0"
  },
  "bin": "cli.js",
  "files": [
    "lib",
    "*.js"
  ],
  "scripts": {
    "coverage": "exit 0",
    "dev": "exit 0",
    "format": "exit 0",
    "lect": "lect",
    "lint": "exit 0",
    "prettier": "exit 0",
    "test": "exit 0",
    "test:tap": "tap --no-esm",
    "unittest": "exit 0"
  },
  "tap": {
    "browser": false,
    "coverage-report": [
      "html",
      "lcov",
      "text"
    ],
    "esm": false,
    "files": [
      "test/*.js",
      "test/reporters/*.js",
      "test/integration/*.js"
    ],
    "flow": false,
    "jobs": 2,
    "jsx": false,
    "timeout": 300,
    "ts": false
  },
