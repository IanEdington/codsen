{
  "name": "object-boolean-combinations",
  "version": "2.0.4",
  "description": "Generate an array full of object copies, each containing a unique Boolean value combination. Includes overrides.",
  "license": "MIT",
  "engines": {
    "node": ">=6",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codsen/object-boolean-combinations.git"
  },
  "bugs": {
    "url": "https://github.com/codsen/object-boolean-combinations/issues"
  },
  "homepage": "https://github.com/codsen/object-boolean-combinations",
  "author": "Roy Revelt <roy@codsen.com> (http://codsen.com/)",
  "main": "dist/object-boolean-combinations.cjs.js",
  "module": "dist/object-boolean-combinations.esm.js",
  "browser": "dist/object-boolean-combinations.umd.js",
  "scripts": {
    "build": "rollup -c",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "rollup -c -w",
    "precommit": "npm test",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/eslint src/*.js test/*.js && nyc --reporter=html --reporter=text ava"
  },
  "ava": {
    "verbose": true
  },
  "clinton": {
    "rules": {
      "xo": 0
    }
  },
  "keywords": [
    "object",
    "unique",
    "permutations",
    "combinations",
    "boolean",
    "generate",
    "array",
    "variations",
    "all",
    "possible",
    "create"
  ],
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "contributors": true,
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "contribution_types": [
      "Answering Questions",
      "Blogposts",
      "Bug reports*",
      "Code*",
      "Design",
      "Documentation*",
      "Event Organizers",
      "Examples",
      "Financial Support",
      "Funding/Grant Finders",
      "Ideas & Planning",
      "Infrastructure (Hosting, Build-Tools, etc)",
      "Plugin/utility libraries",
      "Reviewed Pull Requests",
      "Talks",
      "Tests*",
      "Tools",
      "Translation",
      "Tutorials",
      "Videos"
    ],
    "contributors": [
      {
        "contribution": [
          "Code",
          "Documentation",
          "Tests"
        ],
        "username": "revelt"
      }
    ],
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": ""
    },
    "licence": {
      "extras": []
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      }
    }
  },
  "dependencies": {
    "lodash.clonedeep": "*",
    "lodash.intersection": "*",
    "lodash.isplainobject": "*",
    "lodash.pull": "*"
  },
  "devDependencies": {
    "ava": "*",
    "babel-preset-env": "^1.6.1",
    "coveralls": "*",
    "eslint": "^4.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-ava": "^4.5.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "*",
    "nyc": "*",
    "rollup": "^0.55.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-uglify": "^3.0.0",
    "uglify-es": "^3.3.8"
  }
}
