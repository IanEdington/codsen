{
  "name": "ranges-merge",
  "version": "1.0.3",
  "description": "Merge arrays meaning string slice ranges.",
  "main": "index-es5.js",
  "scripts": {
    "build": "babel index.js --presets babel-preset-es2015 --out-file index-es5.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "prepare": "npm run build",
    "test": "clear && ./node_modules/.bin/eslint index.js test.js && nyc --reporter=html --reporter=text ava && npm run build"
  },
  "repository": "codsen/ranges-merge",
  "keywords": [
    "merge",
    "string",
    "index",
    "ranges",
    "slices",
    "substring",
    "manage",
    "join",
    "two",
    "multiple"
  ],
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/ranges-merge/issues"
  },
  "homepage": "https://github.com/codsen/ranges-merge#readme",
  "dependencies": {
    "ranges-sort": "^2.1.0"
  },
  "devDependencies": {
    "ava": "*",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clear-cli": "*",
    "coveralls": "*",
    "eslint": "^4.7.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "husky": "*",
    "nyc": "*"
  },
  "ava": {
    "verbose": true
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
