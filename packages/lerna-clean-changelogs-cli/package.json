{
  "name": "lerna-clean-changelogs-cli",
  "version": "0.1.0",
  "description": "CLI application to cleanse the lerna-generated changelogs",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "repository": "https://bitbucket.org/codsen/codsen/src/master/packages/lerna-clean-changelogs-cli",
  "homepage": "https://bitbucket.org/codsen/codsen/src/master/packages/lerna-clean-changelogs-cli",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "bin": {
    "lcc": "cli.js",
    "lernacleanchangelog": "cli.js"
  },
  "scripts": {
    "coverage": "nyc report --reporter=json-summary",
    "format": "prettier '*.{js,ts,css,less,scss,vue,gql,md}' --write && npm run lint",
    "lect": "lect",
    "lint": "./node_modules/.bin/eslint **/*.js --fix",
    "test": "npm run lint && npm run unittest",
    "unittest": "./node_modules/.bin/nyc ava && npm run coverage"
  },
  "ava": {
    "compileEnhancements": false,
    "require": [
      "esm"
    ],
    "verbose": true
  },
  "esm": {
    "await": true,
    "cjs": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm test"
    }
  },
  "keywords": [
    "lerna",
    "changelog",
    "manage",
    "manager",
    "utility",
    "fix",
    "patch",
    "monorepo",
    "tool",
    "entries",
    "conventional",
    "cli"
  ],
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "contributors": true,
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "contribution_types": [
      "Blogposts",
      "Bug reports",
      "Code",
      "Design",
      "Documentation",
      "Event Organizing",
      "Examples",
      "Financial",
      "Funding Finding",
      "Ideas, Planning, & Feedback",
      "Infrastructure (Hosting, Build-Tools, etc)",
      "Plugin/utility libraries",
      "Answering Questions",
      "Reviewed Pull Requests",
      "Talks",
      "Tests",
      "Tools",
      "Translation",
      "Tutorials",
      "Videos"
    ],
    "contributors": [
      {
        "contribution": [
          "Code",
          "Documentation",
          "Tests"
        ],
        "username": "revelt"
      }
    ],
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": [
        ""
      ]
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      },
      "devDependencies": [
        "p-map"
      ],
      "travisVersionsOverride": []
    }
  },
  "nyc": {
    "instrument": false,
    "require": [
      "@babel/register"
    ],
    "sourceMap": false
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "fs-extra": "^7.0.1",
    "globby": "^9.0.0",
    "lerna-clean-changelogs": "^1.0.0",
    "meow": "^5.0.0",
    "p-filter": "^1.0.0",
    "p-reduce": "^1.0.0",
    "pify": "^4.0.1",
    "update-notifier": "^2.5.0",
    "write-file-atomic": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "ava": "^1.1.0",
    "babel-plugin-istanbul": "^5.1.0",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^3.5.0",
    "eslint-plugin-ava": "^5.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "eslint-plugin-prettier": "^3.0.1",
    "esm": "^3.1.0",
    "execa": "^1.0.0",
    "nyc": "^13.1.0",
    "p-map": "^2.0.0",
    "prettier": "^1.15.3"
  }
}
