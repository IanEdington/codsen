{
  "name": "ranges-is-index-within",
  "version": "1.1.0",
  "description": "Efficiently checks if index is within any of the given ranges",
  "main": "es5/index.js",
  "scripts": {
    "build": "babel index.js --presets babel-preset-es2015 --out-dir es5",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "prepare": "npm run build",
    "test": "clear && ./node_modules/.bin/eslint index.js test.js && nyc --reporter=html --reporter=text ava && npm run build",
    "watch": "nodemon --quiet --watch . --exec npm run test"
  },
  "repository": "codsen/ranges-is-index-within",
  "keywords": [
    "ranges",
    "index",
    "string",
    "within",
    "is"
  ],
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/ranges-is-index-within/issues"
  },
  "homepage": "https://github.com/codsen/ranges-is-index-within#readme",
  "dependencies": {
    "check-types-mini": "^2.5.0",
    "is-natural-number": "^4.0.1",
    "is-natural-number-string": "^1.0.1",
    "lodash.isplainobject": "*",
    "ordinal-number-suffix": "^0.1.1",
    "ranges-sort": "^2.1.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clear-cli": "*",
    "coveralls": "*",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "husky": "*",
    "nodemon": "*",
    "nyc": "^11.2.1"
  },
  "ava": {
    "verbose": true
  }
}
