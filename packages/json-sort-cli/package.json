{
  "name": "json-sort-cli",
  "version": "1.8.0",
  "description": "Command line app to deep sort JSON files, also dot-files as long as they are valid JSON",
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/codsen/json-sort-cli.git"
  },
  "homepage": "https://bitbucket.org/codsen/json-sort-cli/",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "bin": {
    "jsonsort": "cli.js",
    "sortjson": "cli.js"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "format": "prettier '*.{js,ts,css,less,scss,vue,gql,md}' --write && ./node_modules/.bin/eslint cli.js test/*.js --fix",
    "precommit": "npm run format && npm test",
    "test": "./node_modules/.bin/eslint cli.js && nyc --reporter=html --reporter=text ava"
  },
  "ava": {
    "compileEnhancements": false,
    "require": [
      "esm"
    ],
    "verbose": true
  },
  "clinton": {
    "rules": {
      "xo": 0
    }
  },
  "esm": {
    "await": true,
    "cjs": true
  },
  "keywords": [
    "json",
    "sort",
    "fix",
    "utility",
    "file",
    "files",
    "cli",
    "command line",
    "tool",
    "app"
  ],
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "contributors": true,
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "contribution_types": [
      "Blogposts",
      "Bug reports",
      "Code",
      "Design",
      "Documentation",
      "Event Organizing",
      "Examples",
      "Financial",
      "Funding Finding",
      "Ideas, Planning, & Feedback",
      "Infrastructure (Hosting, Build-Tools, etc)",
      "Plugin/utility libraries",
      "Answering Questions",
      "Reviewed Pull Requests",
      "Talks",
      "Tests",
      "Tools",
      "Translation",
      "Tutorials",
      "Videos"
    ],
    "contributors": [
      {
        "contribution": [
          "Code",
          "Documentation",
          "Tests"
        ],
        "username": "revelt"
      }
    ],
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": [
        "JSON regex used from https://github.com/validate-io/json - Copyright Â© 2015. Athan Reines."
      ]
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      },
      "devDependencies": [],
      "travisVersionsOverride": []
    }
  },
  "dependencies": {
    "ast-monkey-traverse": "^1.3.3",
    "chalk": "latest",
    "fs-extra": "^6.0.1",
    "globby": "latest",
    "is-d": "^1.0.0",
    "lodash.isplainobject": "^4.0.6",
    "meow": "^5.0.0",
    "p-reduce": "^1.0.0",
    "sorted-object": "latest",
    "update-notifier": "latest"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "codacy-coverage": "latest",
    "coveralls": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-ava": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-no-unsanitized": "latest",
    "eslint-plugin-no-wildcard-postmessage": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-scanjs-rules": "latest",
    "esm": "latest",
    "execa": "^0.10.0",
    "husky": "latest",
    "nyc": "latest",
    "p-map": "^1.2.0",
    "prettier": "latest",
    "tempy": "latest"
  }
}
