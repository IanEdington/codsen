{
  "name": "string-replace-slices-array",
  "version": "1.2.0",
  "description": "Delete or replace an array of slices in string",
  "main": "index.js",
  "scripts": {
    "build": "babel index.js --presets babel-preset-es2015 --out-dir es5",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "prepublish": "npm run build",
    "test": "standard  && nyc --reporter=html --reporter=text ava"
  },
  "repository": "codsen/string-replace-slices-array",
  "keywords": [
    "string",
    "delete",
    "replace",
    "multiple",
    "ranges",
    "slice",
    "slices",
    "many",
    "from",
    "splice"
  ],
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/string-replace-slices-array/issues"
  },
  "homepage": "https://github.com/codsen/string-replace-slices-array#readme",
  "dependencies": {
    "is-natural-number": "*",
    "is-natural-number-string": "*",
    "lodash.isplainobject": "*",
    "ordinal-number-suffix": "^0.1.1"
  },
  "devDependencies": {
    "ava": "*",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "*",
    "husky": "*",
    "nyc": "*",
    "standard": "^10.0.3"
  },
  "ava": {
    "verbose": true
  },
  "engines": {
    "node": ">=5.11.0"
  },
  "standard": {
    "ignore": [
      "es5/"
    ]
  }
}
