{
  "name": "string-replace-slices-array",
  "version": "2.0.1",
  "description": "Delete or replace an array of slices in string",
  "license": "MIT",
  "engines": {
    "node": ">=6",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codsen/string-replace-slices-array.git"
  },
  "bugs": {
    "url": "https://github.com/codsen/string-replace-slices-array/issues"
  },
  "homepage": "https://github.com/codsen/string-replace-slices-array#readme",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "main": "dist/string-replace-slices-array.cjs.js",
  "module": "dist/string-replace-slices-array.esm.js",
  "browser": "dist/string-replace-slices-array.umd.js",
  "scripts": {
    "build": "rollup -c",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "rollup -c -w",
    "precommit": "npm test",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/eslint src/*.js test/*.js && nyc --reporter=html --reporter=text ava"
  },
  "ava": {
    "verbose": true
  },
  "clinton": {
    "rules": {
      "xo": 0
    }
  },
  "keywords": [
    "string",
    "delete",
    "replace",
    "multiple",
    "ranges",
    "slice",
    "slices",
    "many",
    "from",
    "splice"
  ],
  "dependencies": {
    "is-natural-number": "*",
    "is-natural-number-string": "*",
    "ordinal-number-suffix": "^0.1.1"
  },
  "devDependencies": {
    "ava": "*",
    "babel-preset-env": "^1.6.1",
    "coveralls": "*",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-ava": "^4.2.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "*",
    "nyc": "*",
    "rollup": "^0.51.1",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "uglify-es": "^3.1.8"
  }
}
