{
  "name": "string-replace-slices-array",
  "version": "1.4.0",
  "description": "Delete or replace an array of slices in string",
  "license": "MIT",
  "engines": {
    "node": ">=0.10.0"
  },
  "repository": "codsen/string-replace-slices-array",
  "bugs": {
    "url": "https://github.com/codsen/string-replace-slices-array/issues"
  },
  "homepage": "https://github.com/codsen/string-replace-slices-array#readme",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "main": "index-es5.js",
  "scripts": {
    "build": "babel index.js --presets babel-preset-es2015 --out-file index-es5.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "prepare": "npm run build",
    "test": "clear && ./node_modules/.bin/eslint index.js test.js && nyc --reporter=html --reporter=text ava && npm run build"
  },
  "ava": {
    "verbose": true
  },
  "keywords": [
    "string",
    "delete",
    "replace",
    "multiple",
    "ranges",
    "slice",
    "slices",
    "many",
    "from",
    "splice"
  ],
  "dependencies": {
    "is-natural-number": "*",
    "is-natural-number-string": "*",
    "ordinal-number-suffix": "^0.1.1"
  },
  "devDependencies": {
    "ava": "*",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clear-cli": "*",
    "coveralls": "*",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "*",
    "nyc": "*"
  }
}
