{
  "name": "object-set-all-values-to",
  "version": "1.1.3",
  "description": "Recursively walk the input and set all found values in plain objects to something",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "test": "standard && nyc --reporter=html --reporter=text ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code-and-send/object-set-all-values-to.git"
  },
  "keywords": [
    "ast",
    "plain",
    "object",
    "posthtml-tree",
    "posthtmltree",
    "set",
    "all",
    "values",
    "to",
    "nested",
    "write"
  ],
  "author": "Roy Reveltas <roy@codeandsend.com> (http://codeandsend.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/code-and-send/object-set-all-values-to/issues"
  },
  "homepage": "https://github.com/code-and-send/object-set-all-values-to#readme",
  "dependencies": {
    "lodash.isplainobject": "^4.0.6"
  },
  "devDependencies": {
    "ava": "*",
    "coveralls": "*",
    "husky": "^0.12.0",
    "standard": "*",
    "nyc": "^10.0.0"
  },
  "ava": {
    "tap": false,
    "verbose": true
  }
}
