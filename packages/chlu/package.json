{
  "name": "chlu",
  "version": "3.2.1",
  "description": "CH-ange-L-og U-pdate - Automatically fix errors in your changelog file",
  "license": "MIT",
  "engines": {
    "node": ">=6",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/codsen/chlu.git"
  },
  "homepage": "https://bitbucket.org/codsen/chlu/",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "main": "dist/chlu.cjs.js",
  "module": "dist/chlu.esm.js",
  "scripts": {
    "build": "rollup -c",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "rollup -c --dev --silent",
    "format": "prettier '*.{js,ts,css,less,scss,vue,gql,md}' --write && ./node_modules/.bin/eslint src/*.js test/*.js --fix",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/eslint src/*.js test/*.js && nyc ava"
  },
  "ava": {
    "compileEnhancements": false,
    "require": [
      "esm"
    ],
    "verbose": true
  },
  "clinton": {
    "rules": {
      "xo": 0
    }
  },
  "esm": {
    "await": true,
    "cjs": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm test"
    }
  },
  "keywords": [
    "update",
    "changelog",
    "change",
    "log",
    "fix",
    "errors",
    "missing",
    "links",
    "markdown",
    "correct",
    "automatically",
    "automated",
    "tool"
  ],
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "contributors": true,
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "contribution_types": [
      "Blogposts",
      "Bug reports",
      "Code",
      "Design",
      "Documentation",
      "Event Organizing",
      "Examples",
      "Financial",
      "Funding Finding",
      "Ideas, Planning, & Feedback",
      "Infrastructure (Hosting, Build-Tools, etc)",
      "Plugin/utility libraries",
      "Answering Questions",
      "Reviewed Pull Requests",
      "Talks",
      "Tests",
      "Tools",
      "Translation",
      "Tutorials",
      "Videos"
    ],
    "contributors": [
      {
        "contribution": [
          "Code",
          "Documentation",
          "Tests"
        ],
        "username": "revelt"
      }
    ],
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": [
        "",
        "Unit test #13 - uses changelog of **giu 0.13.4** to test automated error fixing, released under MIT License (MIT) Copyright © 2016-2018 Guillermo Grau Panea",
        "Unit test #14 - uses changelog of **keystone 4.0.0-beta.5** to test automated error fixing, released under MIT License (MIT) Copyright © 2016 Jed Watson"
      ]
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      },
      "devDependencies": [
        "rollup-plugin-json",
        "rollup-plugin-node-builtins"
      ],
      "travisVersionsOverride": []
    }
  },
  "nyc": {
    "instrument": false,
    "require": [
      "@babel/register"
    ],
    "sourceMap": false
  },
  "dependencies": {
    "ast-contains-only-empty-space": "*",
    "dehumanize-date": "^2.6.2",
    "easy-replace": "^3.2.1",
    "emoji-regex": "^7.0.1",
    "get-pkg-repo": "^3.0.0",
    "is-natural-number": "^4.0.1",
    "just-insert": "^2.1.0",
    "lodash.clonedeep": "*",
    "lodash.includes": "*",
    "lodash.min": "*",
    "lodash.reverse": "*",
    "lodash.trim": "*",
    "semver-compare": "^1.0.0",
    "split-lines": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "ava": "1.0.0-rc.1",
    "babel-plugin-istanbul": "^5.1.0",
    "coveralls": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-ava": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-no-unsanitized": "latest",
    "eslint-plugin-no-wildcard-postmessage": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-scanjs-rules": "latest",
    "husky": "^1.1.2",
    "nyc": "latest",
    "prettier": "latest",
    "rollup": "latest",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "latest",
    "rollup-plugin-node-builtins": "latest",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-strip": "^1.2.0",
    "rollup-plugin-terser": "^3.0.0"
  }
}
