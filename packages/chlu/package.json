{
  "name": "chlu",
  "version": "1.3.1",
  "description": "CH-ange-L-og U-pdate - Automatically fix errors in your changelog file",
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test && doctoc README.md",
    "test": "standard --verbose | snazzy && nyc --reporter=html --reporter=text ava",
    "watch": "nodemon --quiet --watch . --exec npm run test"
  },
  "repository": "codsen/chlu",
  "keywords": [
    "choose",
    "folder",
    "list",
    "cli",
    "pipe",
    "commands"
  ],
  "author": {
    "name": "Roy Reveltas",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/chlu/issues"
  },
  "homepage": "https://github.com/codsen/chlu#readme",
  "dependencies": {
    "dehumanize-date": "^2.4.0",
    "get-pkg-repo": "^1.4.0",
    "is-natural-number": "^4.0.1",
    "just-insert": "^1.0.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.includes": "^4.3.0",
    "lodash.min": "^4.0.1",
    "lodash.reverse": "^4.0.1",
    "lodash.trim": "^4.5.1",
    "posthtml-ast-contains-only-empty-space": "^1.6.0",
    "semver-compare": "^1.0.0",
    "split-lines": "^1.1.0"
  },
  "devDependencies": {
    "ava": "*",
    "coveralls": "*",
    "husky": "^0.14.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.2",
    "snazzy": "^7.0.0",
    "standard": "^10.0.0"
  },
  "ava": {
    "verbose": true
  }
}
