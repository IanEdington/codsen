{
  "name": "giu",
  "private": true,
  "version": "0.13.4",
  "author": "Guillermo Grau Panea",
  "license": "MIT",
  "description": "A collection of React components and utilities",
  "keywords": [
    "React",
    "components",
    "collection",
    "forms",
    "inputs",
    "ssr",
    "i18n"
  ],
  "homepage": "https://github.com/guigrpa/giu#readme",
  "bugs": {
    "url": "https://github.com/guigrpa/giu/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guigrpa/giu.git"
  },
  "scripts": {
    "status": "oao status",
    "travis": "yarn bootstrap && yarn compile",
    "build": "yarn bootstrap && yarn compile && yarn lint && yarn flow && yarn buildExamplesSsr && yarn docs && yarn xxl && yarn prepareRelease",
    "bootstrap": "oao bootstrap",
    "compile": "oao all --parallel \"yarn compile\"",
    "compileWatch": "oao all --parallel \"yarn compileWatch\"",
    "buildExamplesOld": "yarn buildExamplesSsr",
    "buildExamples": "cd packages/giu-examples && yarn build",
    "buildExamplesDev": "cd packages/giu-examples && yarn buildWatch",
    "buildExamplesSsr": "yarn buildExamplesCopy && cross-env NODE_ENV=production SERVER_SIDE_RENDERING=true webpack --config examples/webpackConfig.js --color --progress --display-chunks -p",
    "buildExamplesSsrDev": "yarn buildExamplesCopy && cross-env SERVER_SIDE_RENDERING=true webpack --config examples/webpackConfig.js --color --progress --display-chunks --watch",
    "buildExamplesCopy": "cp -r examples/stylesheets docs/ && cp examples/favicon.ico docs/",
    "start": "cd packages/giu-examples && yarn example",
    "lint": "eslint packages",
    "flow": "flow check || exit 0",
    "docs": "extract-docs --template docs/templates/README.md --output README.md",
    "xxl": "xxl --src packages --exclude node_modules,/lib/",
    "jest": "jest --watch --coverage",
    "jest-html": "jest-html --css-patterns test/snapshot.css --snapshot-patterns \"packages/**/*.snap\"",
    "test": "yarn testCovFull",
    "testFast": "jest",
    "testCovFull": "yarn _testCovPrepare && yarn _testDev && yarn _testProd && yarn _testCovReport",
    "testCovFast": "yarn _testCovPrepare && yarn _testDev && yarn _testCovReport",
    "_testCovPrepare": "rm -rf ./coverage .nyc_output .nyc_tmp && mkdir .nyc_tmp",
    "_testCovReport": "cp -r .nyc_tmp .nyc_output && nyc report --reporter=html --reporter=lcov --reporter=text",
    "_testDev": "cross-env NODE_ENV=development yarn _testCov && mv .nyc_tmp/coverage-final.json .nyc_tmp/coverage-dev.json",
    "_testProd": "cross-env NODE_ENV=production yarn _testCov && mv .nyc_tmp/coverage-final.json .nyc_tmp/coverage-prod.json",
    "_testCov": "jest --coverage && mv .nyc_output/coverage-final.json .nyc_tmp && rm -rf .nyc_output",
    "prepareRelease": "oao prepublish",
    "release": "oao publish"
  },
  "_devDependencies": "The following are included to avoid dep conflicts: react, react-dom, moment",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "17.0.2",
    "babel-loader": "7.0.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "coveralls": "2.13.1",
    "cross-env": "3.1.4",
    "css-loader": "0.28.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-flowtype": "2.32.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "extract-docs": "^1.5.2",
    "file-loader": "0.11.1",
    "flow-bin": "0.45.0",
    "flow-copy-source": "1.1.0",
    "jest": "17.0.3",
    "jest-html": "^1.3.4",
    "moment": "2.18.1",
    "nyc": "8.4.0",
    "oao": "^0.8.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "static-site-generator-webpack-plugin": "2.1.0",
    "style-loader": "0.17.0",
    "webpack": "2.4.1",
    "xxl": "1.2.0"
  },
  "jest": {
    "testRegex": "packages/.*__tests__/.*\\.(test|spec)\\.(js|jsx)$",
    "moduleNameMapper": {
      "^.+\\.(css|less|sass)$": "<rootDir>/test/emptyObject.js",
      "^.+\\.(gif|ttf|eot|svg)$": "<rootDir>/test/emptyString.js"
    },
    "coverageDirectory": ".nyc_output",
    "coverageReporters": [
      "json",
      "text",
      "html"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-html"
    ],
    "collectCoverageFrom": [
      "packages/*/src/**",
      "!packages/giu-examples/**",
      "!**/node_modules/**",
      "!**/__tests__/**",
      "!**/__mocks__/**"
    ],
    "setupTestFrameworkScriptFile": "./test/setup.js"
  }
}
