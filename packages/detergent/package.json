{
  "name": "detergent",
  "version": "5.8.1",
  "description": "a tool to prepare text for pasting into HTML",
  "license": "MIT",
  "engines": {
    "node": ">=8.9"
  },
  "repository": "https://gitlab.com/codsen/codsen/",
  "homepage": "https://detergent.io",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "keywords": [
    "brief",
    "clean",
    "code",
    "copy",
    "detergent",
    "email",
    "encode",
    "html",
    "send",
    "string",
    "style",
    "text"
  ],
  "main": "dist/detergent.cjs.js",
  "module": "dist/detergent.esm.js",
  "browser": "dist/detergent.umd.js",
  "scripts": {
    "build": "rollup -c --silent",
    "dev": "rollup -c --dev --silent",
    "devunittest": "npm run dev && ./node_modules/.bin/tap --only",
    "format": "npm run lect && npm run prettier && npm run lint",
    "lect": "lect",
    "lint": "../../node_modules/eslint/bin/eslint.js \"**/*.js\" --fix --ignore-pattern \"dist/*\" --ignore-pattern \"rollup.config.js\" --ignore-pattern \"tap/**\"",
    "perf": "node perf/check",
    "prepare": "npm run build",
    "prettier": "../../node_modules/prettier/bin-prettier.js '*.{js,css,scss,vue,md}' --write",
    "tap": "tap",
    "pretest": "npm run build",
    "test": "npm run lint && npm run unittest && npm run format",
    "unittest": "./node_modules/.bin/tap --no-only -R 'terse' | tail -n 2 | tee testStats.md && npm run perf",
    "version": "npm run build && git add ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm test"
    }
  },
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": false,
      "vulnerabilities": true
    },
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": [
        "",
        "Passes unit tests from https://github.com/kemitchell/straight-to-curly-quotes.json, licenced under CC0-1.0"
      ]
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "req": "{ det, opts, version }",
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      },
      "devDependencies": [
        "is-ci",
        "lodash.clonedeep",
        "object-boolean-combinations",
        "object-path",
        "rollup-plugin-json",
        "rollup-plugin-node-builtins"
      ],
      "rollupIgnoreFilesForDist": "processCharacter.js",
      "travisVersionsOverride": []
    }
  },
  "tap": {
    "coverage-report": "json-summary",
    "timeout": 0
  },
  "dependencies": {
    "all-named-html-entities": "^1.2.13",
    "ansi-regex": "^5.0.0",
    "ast-deep-contains": "^1.1.0",
    "he": "^1.2.0",
    "html-entities-not-email-friendly": "^0.1.10",
    "lodash.isplainobject": "^4.0.6",
    "ranges-apply": "^3.0.43",
    "ranges-invert": "^2.1.27",
    "ranges-process-outside": "^2.2.11",
    "ranges-push": "^3.6.10",
    "string-apostrophes": "^1.2.6",
    "string-collapse-white-space": "^5.2.5",
    "string-fix-broken-named-entities": "^2.5.1",
    "string-left-right": "^2.3.10",
    "string-range-expander": "^1.10.50",
    "string-remove-widows": "^1.5.6",
    "string-strip-html": "^4.3.9",
    "string-trim-spaces-only": "^2.8.7"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "benchmark": "^2.1.4",
    "is-ci": "^2.0.0",
    "js-row-num-cli": "^1.6.9",
    "lect": "^0.11.2",
    "lodash.clonedeep": "^4.5.0",
    "object-boolean-combinations": "^2.11.49",
    "object-path": "^0.11.4",
    "rollup": "^1.27.8",
    "rollup-plugin-ascii": "^0.0.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-strip": "^1.2.2",
    "rollup-plugin-terser": "^5.1.2",
    "tap": "^14.10.2"
  }
}
