{
  "name": "update-versions",
  "version": "1.2.1",
  "description": "Like npm-check-updates but supports Lerna monorepos and enforces strict semver values",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "repository": "https://bitbucket.org/codsen/update-versions",
  "homepage": "https://bitbucket.org/codsen/update-versions",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "bin": {
    "upd": "cli.js"
  },
  "scripts": {
    "coverage": "nyc report --reporter=json-summary",
    "format": "prettier '*.{js,ts,css,less,scss,vue,gql,md}' --write && npm run lint",
    "lect": "lect",
    "lint": "./node_modules/.bin/eslint **/*.js --fix",
    "test": "npm run lint && npm run unittest",
    "unittest": "./node_modules/.bin/nyc ava && npm run coverage"
  },
  "ava": {
    "compileEnhancements": false,
    "require": [
      "esm"
    ],
    "verbose": true
  },
  "esm": {
    "await": true,
    "cjs": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm test"
    }
  },
  "keywords": [
    "update",
    "versions",
    "package",
    "json",
    "package.json",
    "npm",
    "lerna",
    "monorepo",
    "friendly",
    "check",
    "updates",
    "automates",
    "tool",
    "app",
    "cli",
    "utility",
    "upgrade",
    "dependencies",
    "version"
  ],
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "contributors": true,
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "contribution_types": [
      "Blogposts",
      "Bug reports",
      "Code",
      "Design",
      "Documentation",
      "Event Organizing",
      "Examples",
      "Financial",
      "Funding Finding",
      "Ideas, Planning, & Feedback",
      "Infrastructure (Hosting, Build-Tools, etc)",
      "Plugin/utility libraries",
      "Answering Questions",
      "Reviewed Pull Requests",
      "Talks",
      "Tests",
      "Tools",
      "Translation",
      "Tutorials",
      "Videos"
    ],
    "contributors": [
      {
        "contribution": [
          "Code",
          "Documentation",
          "Tests"
        ],
        "username": "revelt"
      }
    ],
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": [
        ""
      ]
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      },
      "devDependencies": [
        "p-map",
        "lodash.clonedeep"
      ],
      "travisVersionsOverride": []
    }
  },
  "nyc": {
    "instrument": false,
    "require": [
      "@babel/register"
    ],
    "sourceMap": false
  },
  "dependencies": {
    "format-package": "^4.5.0",
    "fs-extra": "^7.0.1",
    "globby": "^9.0.0",
    "lodash.isplainobject": "^4.0.6",
    "log-symbols": "^2.2.0",
    "log-update": "^2.3.0",
    "meow": "^5.0.0",
    "pacote": "^9.4.1",
    "path": "^0.12.7",
    "update-notifier": "^2.5.0",
    "write-json-file": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "ava": "^1.2.0",
    "babel-plugin-istanbul": "^5.1.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-ava": "^5.1.1",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "eslint-plugin-no-wildcard-postmessage": "^0.1.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "esm": "^3.1.4",
    "execa": "^1.0.0",
    "husky": "^1.3.1",
    "lodash.clonedeep": "^4.5.0",
    "nyc": "^13.1.0",
    "p-map": "^2.0.0",
    "prettier": "^1.16.1",
    "tempy": "^0.2.1"
  }
}
