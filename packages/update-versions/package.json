{
  "name": "update-versions",
  "version": "2.4.13",
  "description": "Like npm-check-updates but supports Lerna monorepos and enforces strict semver values",
  "license": "MIT",
  "repository": "https://gitlab.com/codsen/codsen/",
  "homepage": "https://gitlab.com/codsen/codsen/tree/master/packages/update-versions",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "keywords": [
    "app",
    "automates",
    "check",
    "cli",
    "dependencies",
    "friendly",
    "json",
    "lerna",
    "monorepo",
    "npm",
    "package",
    "package.json",
    "tool",
    "update",
    "updates",
    "upgrade",
    "utility",
    "version",
    "versions"
  ],
  "bin": {
    "upd": "cli.js"
  },
  "scripts": {
    "ci_test": "tap --no-only --reporter=silent --output-file=testStats.md && npm run format",
    "dev": "echo\"\"",
    "devunittest": "npm run dev && ./node_modules/.bin/tap --only -R 'base'",
    "format": "npm run lect && npm run prettier && npm run lint",
    "lect": "lect",
    "lint": "../../node_modules/eslint/bin/eslint.js \"**/*.js\" --fix --config \"../../.eslintrc.json\" --quiet",
    "prettier": "../../node_modules/prettier/bin-prettier.js '*.{js,css,scss,vue,md}' --write --loglevel silent",
    "republish": "npm publish || :",
    "tap": "tap",
    "test": "npm run lint && npm run unittest && npm run format",
    "unittest": "./node_modules/.bin/tap --no-only --output-file=testStats.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm test"
    }
  },
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": [
        "uses adapted p-progress, MIT Licence, Copyright (c) Sindre Sorhus https://github.com/sindresorhus/p-progress/blob/master/license"
      ]
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": [
        "util"
      ]
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      },
      "devDependencies": [
        "fs-extra",
        "lodash.clonedeep"
      ],
      "travisVersionsOverride": []
    }
  },
  "tap": {
    "coverage-report": [
      "json-summary",
      "text"
    ],
    "nyc-arg": [
      "--exclude=**/*.umd.js"
    ],
    "timeout": 0
  },
  "dependencies": {
    "ansi-diff-stream": "^1.2.1",
    "edit-package-json": "^0.1.27",
    "globby": "^11.0.0",
    "is-online": "^8.3.1",
    "lodash.isplainobject": "^4.0.6",
    "log-update": "^4.0.0",
    "meow": "^7.0.1",
    "object-path": "^0.11.4",
    "p-map": "^4.0.0",
    "p-progress": "^0.4.2",
    "p-reduce": "^2.1.0",
    "pacote": "^11.1.10",
    "update-notifier": "^4.1.0",
    "write-file-atomic": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-strip": "^1.3.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-no-unsanitized": "^3.1.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-row-num": "^1.2.4",
    "execa": "^4.0.1",
    "fs-extra": "^9.0.0",
    "lect": "^0.13.4",
    "lodash.clonedeep": "^4.5.0",
    "prettier": "^2.0.5",
    "tap": "^14.10.7",
    "tempy": "^0.5.0"
  }
}
