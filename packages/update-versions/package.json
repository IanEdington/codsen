{
  "name": "update-versions",
  "version": "2.3.0",
  "description": "Like npm-check-updates but supports Lerna monorepos and enforces strict semver values",
  "license": "MIT",
  "engines": {
    "node": ">=8.9"
  },
  "repository": "https://gitlab.com/codsen/codsen/",
  "homepage": "https://gitlab.com/codsen/codsen/tree/master/packages/update-versions",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "keywords": [
    "app",
    "automates",
    "check",
    "cli",
    "dependencies",
    "friendly",
    "json",
    "lerna",
    "monorepo",
    "npm",
    "package",
    "package.json",
    "tool",
    "update",
    "updates",
    "upgrade",
    "utility",
    "version",
    "versions"
  ],
  "bin": {
    "upd": "cli.js"
  },
  "scripts": {
    "dev": "echo\"\"",
    "format": "npm run lect && npm run prettier && npm run lint",
    "lect": "lect",
    "lint": "../../node_modules/eslint/bin/eslint.js \"**/*.js\" --fix --ignore-pattern \"dist/*\" --ignore-pattern \"rollup.config.js\" --ignore-pattern \"tap/**\"",
    "prettier": "../../node_modules/prettier/bin-prettier.js '*.{js,css,scss,vue,md}' --write",
    "tap": "tap",
    "test": "npm run lint && npm run unittest && npm run format",
    "unittest": "./node_modules/.bin/tap --no-only -R 'terse'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm test"
    }
  },
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": [
        ""
      ]
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      },
      "devDependencies": [
        "fs-extra",
        "lodash.clonedeep",
        "p-map"
      ],
      "travisVersionsOverride": []
    }
  },
  "tap": {
    "coverage-report": "json-summary",
    "timeout": 0
  },
  "dependencies": {
    "ansi-diff-stream": "^1.2.1",
    "edit-package-json": "^0.1.11",
    "globby": "^10.0.1",
    "is-online": "^8.2.1",
    "lodash.isplainobject": "^4.0.6",
    "log-update": "^3.3.0",
    "meow": "^5.0.0",
    "object-path": "^0.11.4",
    "p-progress": "^0.4.1",
    "p-reduce": "^2.1.0",
    "pacote": "^10.2.0",
    "update-notifier": "^3.0.1",
    "write-file-atomic": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-row-num": "^1.0.0",
    "execa": "^3.4.0",
    "fs-extra": "^8.1.0",
    "js-row-num-cli": "^1.6.9",
    "lect": "^0.11.2",
    "lodash.clonedeep": "^4.5.0",
    "p-map": "^3.0.0",
    "prettier": "^1.19.1",
    "tap": "^14.10.2",
    "tempy": "^0.3.0"
  }
}
