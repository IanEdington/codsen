{
  "name": "string-unfancy",
  "version": "2.0.2",
  "description": "Replace all fancy dashes, quotes etc with their simpler equivalents",
  "main": "index.js",
  "scripts": {
    "build": "babel index.js --presets babel-preset-es2015 --out-dir es5",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "prepare": "npm run build",
    "test": "standard && nyc --reporter=html --reporter=text ava"
  },
  "repository": "codsen/string-unfancy",
  "keywords": [
    "string",
    "characters",
    "replace",
    "email",
    "templates",
    "alt",
    "tags",
    "attributes",
    "fancy",
    "simple",
    "quote",
    "dash",
    "remove",
    "encoding"
  ],
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/string-unfancy/issues"
  },
  "homepage": "https://github.com/codsen/string-unfancy#readme",
  "dependencies": {
    "he": "^1.1.1"
  },
  "devDependencies": {
    "ava": "*",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "*",
    "husky": "*",
    "nyc": "*",
    "standard": "^10.0.3"
  },
  "ava": {
    "verbose": false
  },
  "engines": {
    "node": ">=5.11.0"
  },
  "standard": {
    "ignore": [
      "es5/"
    ]
  }
}
