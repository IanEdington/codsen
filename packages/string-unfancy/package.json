{
  "name": "string-unfancy",
  "version": "2.1.1",
  "description": "Replace all fancy dashes, quotes etc with their simpler equivalents",
  "main": "index-es5.js",
  "scripts": {
    "build": "babel index.js --presets babel-preset-es2015 --out-file index-es5.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "prepare": "npm run build",
    "test": "clear && ./node_modules/.bin/eslint index.js test.js && nyc --reporter=html --reporter=text ava && npm run build"
  },
  "repository": "codsen/string-unfancy",
  "keywords": [
    "string",
    "characters",
    "replace",
    "email",
    "templates",
    "alt",
    "tags",
    "attributes",
    "fancy",
    "simple",
    "quote",
    "dash",
    "remove",
    "encoding"
  ],
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/string-unfancy/issues"
  },
  "homepage": "https://github.com/codsen/string-unfancy#readme",
  "dependencies": {
    "he": "^1.1.1"
  },
  "devDependencies": {
    "ava": "*",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clear-cli": "*",
    "coveralls": "*",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "husky": "*",
    "nyc": "*"
  },
  "ava": {
    "verbose": false
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
