{
  "name": "object-flatten-referencing",
  "version": "3.5.0",
  "description": "Flatten complex nested objects according to a reference objects",
  "license": "MIT",
  "engines": {
    "node": ">=6",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "repository": "codsen/object-flatten-referencing",
  "bugs": {
    "url": "https://github.com/codsen/object-flatten-referencing/issues"
  },
  "homepage": "https://github.com/codsen/object-flatten-referencing#readme",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "main": "dist/object-flatten-referencing.cjs.js",
  "module": "dist/object-flatten-referencing.esm.js",
  "browser": "dist/object-flatten-referencing.umd.js",
  "scripts": {
    "build": "rollup -c",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "rollup -c -w",
    "precommit": "npm test",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/eslint src/main.js && nyc --reporter=html --reporter=text ava"
  },
  "ava": {
    "verbose": true
  },
  "clinton": {
    "rules": {
      "xo": 0
    }
  },
  "keywords": [
    "object",
    "plain",
    "wrap",
    "flatten",
    "flat",
    "mapping",
    "content",
    "objects",
    "advanced",
    "email"
  ],
  "dependencies": {
    "check-types-mini": "^2.6.0",
    "is-string-int": "^1.0.1",
    "lodash.clonedeep": "*",
    "lodash.includes": "*",
    "matcher": "^1.0.0",
    "str-indexes-of-plus": "^1.0.3",
    "type-detect": "^4.0.3"
  },
  "devDependencies": {
    "ava": "*",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "coveralls": "*",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.7.0",
    "husky": "*",
    "nyc": "*",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "uglify-es": "^3.1.3"
  }
}
