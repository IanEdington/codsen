{
  "name": "object-flatten-referencing",
  "version": "3.5.0",
  "description": "Flatten complex nested objects according to a reference objects",
  "license": "MIT",
  "engines": {
    "node": ">=6",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "repository": "codsen/object-flatten-referencing",
  "bugs": {
    "url": "https://github.com/codsen/object-flatten-referencing/issues"
  },
  "homepage": "https://github.com/codsen/object-flatten-referencing#readme",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "main": "index-es5.js",
  "scripts": {
    "build": "babel index.js --presets babel-preset-env --out-file index-es5.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/eslint index.js && nyc --reporter=html --reporter=text ava"
  },
  "ava": {
    "verbose": false
  },
  "keywords": [
    "object",
    "plain",
    "wrap",
    "flatten",
    "flat",
    "mapping",
    "content",
    "objects",
    "advanced",
    "email"
  ],
  "dependencies": {
    "check-types-mini": "^2.6.0",
    "is-string-int": "^1.0.1",
    "lodash.clonedeep": "*",
    "lodash.includes": "*",
    "matcher": "^1.0.0",
    "str-indexes-of-plus": "^1.0.3",
    "type-detect": "^4.0.3"
  },
  "devDependencies": {
    "ava": "*",
    "babel-cli": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "coveralls": "*",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.7.0",
    "husky": "*",
    "nyc": "^11.2.1"
  }
}
