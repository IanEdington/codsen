{
  "name": "lect",
  "version": "0.5.15",
  "description": "Maintenance automation for Readme's and, optionally, other project files",
  "license": "MIT",
  "engines": {
    "node": ">=8.9"
  },
  "repository": "https://gitlab.com/codsen/codsen/",
  "homepage": "https://gitlab.com/codsen/codsen/tree/master/packages/lect",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "bin": {
    "lect": "cli.js"
  },
  "scripts": {
    "coverage": "nyc report --reporter=json-summary",
    "dev": "echo\"\"",
    "format": "npm run lect && npm run prettier && npm run lint",
    "lect": "lect",
    "lint": "./node_modules/.bin/eslint **/*.js --fix",
    "prettier": "prettier '*.{js,css,scss,vue,md}' --write",
    "test": "npm run lint && npm run unittest && npm run format",
    "unittest": "./node_modules/.bin/nyc ava && npm run coverage"
  },
  "ava": {
    "compileEnhancements": false,
    "require": [
      "esm"
    ],
    "timeout": "99m",
    "verbose": true
  },
  "esm": {
    "await": true,
    "cjs": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm test"
    }
  },
  "keywords": [
    "update",
    "readme",
    "files",
    "file",
    "change",
    "refresh",
    "latest",
    "amend",
    "programatically",
    "maintain",
    "maintenance",
    "tool",
    "edit",
    "cli",
    "cli-app",
    "automatically",
    "automagically",
    "lect"
  ],
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "contributors": true,
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "contribution_types": [
      "Blogposts",
      "Bug reports",
      "Code",
      "Design",
      "Documentation",
      "Event Organizing",
      "Examples",
      "Financial",
      "Funding Finding",
      "Ideas, Planning, & Feedback",
      "Infrastructure (Hosting, Build-Tools, etc)",
      "Plugin/utility libraries",
      "Answering Questions",
      "Reviewed Pull Requests",
      "Talks",
      "Tests",
      "Tools",
      "Translation",
      "Tutorials",
      "Videos"
    ],
    "contributors": [
      {
        "contribution": [
          "Code",
          "Documentation",
          "Tests"
        ],
        "username": "revelt"
      }
    ],
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": [
        "",
        "Icons mapping in util.js/`defaultTypes` taken from [all-contributors-cli](https://github.com/jfmengels/all-contributors-cli). MIT Licence."
      ]
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      },
      "devDependencies": [],
      "travisVersionsOverride": []
    }
  },
  "nyc": {
    "instrument": false,
    "require": [
      "@babel/register"
    ],
    "sourceMap": false
  },
  "dependencies": {
    "array-pull-all-with-glob": "^4.12.21",
    "arrayiffy-if-string": "^3.10.21",
    "ast-monkey-traverse": "^1.11.17",
    "bitbucket-slug": "^1.9.20",
    "chalk": "^2.4.2",
    "execa": "^1.0.0",
    "filesize": "^4.1.2",
    "find-file-recursively-up": "^1.0.2",
    "format-package": "^4.5.0",
    "fs-extra": "^7.0.1",
    "get-pkg-repo": "^4.1.0",
    "github-slugger": "^1.2.1",
    "inquirer": "^6.3.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.partition": "^4.6.0",
    "lodash.trim": "^4.5.1",
    "lodash.uniq": "^4.5.0",
    "log-symbols": "^2.2.0",
    "matcher": "^1.1.1",
    "meow": "^5.0.0",
    "object-delete-key": "^1.8.21",
    "object-merge-advanced": "^10.10.21",
    "object-path": "^0.11.4",
    "p-map": "^2.1.0",
    "p-reduce": "^2.1.0",
    "pacote": "^9.5.0",
    "pify": "^4.0.1",
    "replace-string": "^2.0.0",
    "request": "^2.88.0",
    "semver-compare": "^1.0.0",
    "semver-regex": "^2.0.0",
    "split-lines": "^2.0.0",
    "update-notifier": "^2.5.0",
    "write-file-atomic": "^2.4.2",
    "write-json-file": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "ava": "^1.4.1",
    "babel-plugin-istanbul": "^5.1.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-ava": "^6.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "eslint-plugin-prettier": "^3.0.1",
    "esm": "^3.2.22",
    "js-row-num-cli": "^1.5.21",
    "lect": "^0.5.15",
    "nyc": "^14.0.0",
    "prettier": "^1.17.0",
    "tempy": "^0.3.0"
  }
}
