{
  "name": "lect",
  "version": "0.12.5",
  "description": "Maintenance CLI for internal consumption",
  "license": "MIT",
  "repository": "https://gitlab.com/codsen/codsen/",
  "homepage": "https://gitlab.com/codsen/codsen/tree/master/packages/lect",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "keywords": [
    "amend",
    "automagically",
    "automatically",
    "change",
    "cli",
    "cli-app",
    "edit",
    "file",
    "files",
    "latest",
    "lect",
    "maintain",
    "maintenance",
    "programatically",
    "readme",
    "refresh",
    "tool",
    "update"
  ],
  "bin": {
    "lect": "cli.js"
  },
  "scripts": {
    "dev": "echo\"\"",
    "devunittest": "npm run dev && ./node_modules/.bin/tap --only",
    "format": "npm run lect && npm run prettier && npm run lint",
    "lect": "lect",
    "lint": "../../node_modules/eslint/bin/eslint.js \"**/*.js\" --fix --config \"../../.eslintrc.json\"",
    "prettier": "../../node_modules/prettier/bin-prettier.js '*.{js,css,scss,vue,md}' --write",
    "tap": "tap",
    "test": "npm run lint && npm run unittest && npm run format",
    "unittest": "./node_modules/.bin/tap --no-only -R 'terse' -o testStats.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm test"
    }
  },
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": [
        "",
        "Icons mapping in util.js/`defaultTypes` taken from [all-contributors-cli](https://github.com/jfmengels/all-contributors-cli). MIT Licence."
      ]
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "req": "includesWithGlob",
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      },
      "devDependencies": [],
      "travisVersionsOverride": []
    }
  },
  "tap": {
    "coverage-report": "json-summary",
    "timeout": 0
  },
  "dependencies": {
    "array-pull-all-with-glob": "^4.12.63",
    "arrayiffy-if-string": "^3.11.29",
    "ast-monkey-traverse": "^1.12.8",
    "bitbucket-slug": "^1.9.56",
    "chalk": "^4.0.0",
    "execa": "^4.0.0",
    "filesize": "^6.1.0",
    "find-file-recursively-up": "^1.1.2",
    "format-package": "^5.2.0",
    "fs-extra": "^9.0.0",
    "get-pkg-repo": "^4.1.0",
    "github-slugger": "^1.3.0",
    "inquirer": "^7.1.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.partition": "^4.6.0",
    "lodash.trim": "^4.5.1",
    "lodash.uniq": "^4.5.0",
    "log-symbols": "^3.0.0",
    "matcher": "^2.1.0",
    "meow": "^6.1.0",
    "object-delete-key": "^1.9.21",
    "object-merge-advanced": "^10.11.16",
    "object-path": "^0.11.4",
    "p-map": "^4.0.0",
    "p-reduce": "^2.1.0",
    "request": "^2.88.2",
    "semver-compare": "^1.0.0",
    "semver-regex": "^3.1.1",
    "update-notifier": "^4.1.0",
    "write-file-atomic": "^3.0.3",
    "write-json-file": "^4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-strip": "^1.3.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-no-unsanitized": "^3.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-row-num": "^1.1.10",
    "lect": "^0.12.5",
    "prettier": "^2.0.4",
    "tap": "^14.10.7",
    "tempy": "^0.5.0"
  }
}
