{
  "name": "json-comb-core",
  "version": "5.1.3",
  "description": "The inner core of json-comb",
  "license": "MIT",
  "engines": {
    "node": ">=6",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codsen/json-comb-core.git"
  },
  "bugs": {
    "url": "https://github.com/codsen/json-comb-core/issues"
  },
  "homepage": "https://github.com/codsen/json-comb-core#readme",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "main": "dist/json-comb-core.cjs.js",
  "module": "dist/json-comb-core.esm.js",
  "browser": "dist/json-comb-core.umd.js",
  "scripts": {
    "build": "rollup -c",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "rollup -c -w",
    "precommit": "npm test",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/eslint src/*.js test/*.js && nyc --reporter=html --reporter=text ava"
  },
  "ava": {
    "verbose": true
  },
  "clinton": {
    "rules": {
      "xo": 0
    }
  },
  "keywords": [
    "json",
    "normalize",
    "normalise",
    "schema",
    "files",
    "fill",
    "keyset",
    "keys",
    "same",
    "comb"
  ],
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "contributors": true,
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "contribution_types": [
      "Blogposts",
      "Bug reports",
      "Code",
      "Design",
      "Documentation",
      "Event Organizing",
      "Examples",
      "Financial",
      "Funding Finding",
      "Ideas, Planning, & Feedback",
      "Infrastructure (Hosting, Build-Tools, etc)",
      "Plugin/utility libraries",
      "Answering Questions",
      "Reviewed Pull Requests",
      "Talks",
      "Tests",
      "Tools",
      "Translation",
      "Tutorials",
      "Videos"
    ],
    "contributors": [
      {
        "contribution": [
          "Code",
          "Documentation",
          "Tests"
        ],
        "username": "revelt"
      }
    ],
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": ""
    },
    "licence": {
      "extras": []
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      }
    }
  },
  "dependencies": {
    "check-types-mini": "^3.1.2",
    "lodash.clonedeep": "*",
    "lodash.includes": "*",
    "object-fill-missing-keys": "^6.0.3",
    "object-flatten-all-arrays": "^4.0.6",
    "object-merge-advanced": "^10.0.0",
    "object-no-new-keys": "^2.0.5",
    "object-set-all-values-to": "^3.0.5",
    "p-map": "^1.2.0",
    "p-one": "^1.0.0",
    "p-reduce": "^1.0.0",
    "sort-keys": "^2.0.0",
    "type-detect": "^4.0.8"
  },
  "devDependencies": {
    "ava": "*",
    "babel-preset-env": "^1.6.1",
    "coveralls": "*",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-ava": "^4.5.1",
    "eslint-plugin-import": "^2.9.0",
    "husky": "*",
    "nyc": "*",
    "rollup": "^0.56.5",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.2.0",
    "rollup-plugin-uglify": "^3.0.0",
    "uglify-es": "^3.3.9"
  }
}
