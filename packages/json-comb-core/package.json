{
  "name": "json-comb-core",
  "version": "2.3.2",
  "description": "The inner core of json-comb",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test && doctoc readme.md",
    "test": "standard && nyc --reporter=html --reporter=text ava",
    "watch": "nodemon --quiet --watch . --exec npm run test"
  },
  "repository": "codsen/json-comb-core",
  "keywords": [
    "json",
    "normalize",
    "normalise",
    "schema",
    "files",
    "fill",
    "keyset",
    "keys",
    "same",
    "comb"
  ],
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/json-comb-core/issues"
  },
  "homepage": "https://github.com/codsen/json-comb-core#readme",
  "dependencies": {
    "check-types-mini": "^2.3.0",
    "lodash.clonedeep": "*",
    "object-assign": "*",
    "object-fill-missing-keys": "^2.1.2",
    "object-flatten-all-arrays": "^3.1.2",
    "object-merge-advanced": "^6.5.0",
    "object-no-new-keys": "^1.1.0",
    "object-set-all-values-to": "^2.0.2",
    "type-detect": "^4.0.3"
  },
  "devDependencies": {
    "ava": "*",
    "coveralls": "*",
    "husky": "^0.14.3",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.3",
    "standard": "^10.0.2"
  },
  "ava": {
    "verbose": false
  },
  "engines": {
    "node": ">=5.11.0"
  }
}
