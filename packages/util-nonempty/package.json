{
  "name": "util-nonempty",
  "version": "1.4.0",
  "description": "Is the input (plain object, array, string or whatever) not empty?",
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "repository": "codsen/util-nonempty",
  "bugs": {
    "url": "https://github.com/codsen/util-nonempty/issues"
  },
  "homepage": "https://github.com/codsen/util-nonempty#readme",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "main": "dist/util-nonempty.cjs.js",
  "module": "dist/util-nonempty.esm.js",
  "browser": "dist/util-nonempty.umd.js",
  "scripts": {
    "build": "rollup -c && npm run format:pkg",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "rollup -c -w",
    "format:pkg": "format-package -w -q",
    "precommit": "npm test",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/eslint src/main.js && nyc --reporter=html --reporter=text ava"
  },
  "ava": {
    "verbose": true
  },
  "clinton": {
    "rules": {
      "xo": 0
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "is",
    "not",
    "empty",
    "object",
    "array",
    "string",
    "util"
  ],
  "dependencies": {
    "lodash.isplainobject": "*"
  },
  "devDependencies": {
    "ava": "*",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.0",
    "coveralls": "*",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "format-package": "^3.0.2",
    "husky": "*",
    "nyc": "*",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0"
  }
}
