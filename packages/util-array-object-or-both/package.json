{
  "name": "util-array-object-or-both",
  "version": "1.2.0",
  "description": "Validate and normalise user choice: array, object or both?",
  "main": "index-es5.js",
  "scripts": {
    "build": "babel index.js --presets babel-preset-es2015 --out-file index-es5.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "prepare": "npm run build",
    "test": "clear && ./node_modules/.bin/eslint index.js test.js && nyc --reporter=html --reporter=text ava && npm run build",
    "watch": "nodemon --quiet --watch . --exec npm run test"
  },
  "repository": "codsen/util-array-object-or-both",
  "keywords": [
    "validate",
    "array",
    "plain",
    "object",
    "both",
    "user",
    "util",
    "normalise",
    "choice"
  ],
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/util-array-object-or-both/issues"
  },
  "homepage": "https://github.com/codsen/util-array-object-or-both#readme",
  "dependencies": {
    "check-types-mini": "^2.6.0",
    "lodash.includes": "*",
    "lodash.isplainobject": "*"
  },
  "devDependencies": {
    "ava": "*",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clear-cli": "*",
    "coveralls": "*",
    "eslint": "^4.7.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "husky": "*",
    "nodemon": "*",
    "nyc": "*"
  },
  "ava": {
    "verbose": true
  }
}
