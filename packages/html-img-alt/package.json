{
  "name": "html-img-alt",
  "version": "1.3.0",
  "description": "Adds missing ALT attributes to IMG tags and cleans within IMG tags. No HTML parsing used.",
  "main": "index-es5.js",
  "scripts": {
    "build": "babel index.js --presets babel-preset-es2015 --out-file index-es5.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "prepare": "npm run build",
    "test": "clear && ./node_modules/.bin/eslint index.js test.js && nyc --reporter=html --reporter=text ava && npm run build"
  },
  "repository": "codsen/html-img-alt",
  "keywords": [
    "add",
    "email",
    "template",
    "newsletter",
    "marketing",
    "alt",
    "tags",
    "attributes",
    "missing",
    "fill",
    "templates",
    "restore",
    "clean",
    "parsing"
  ],
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/html-img-alt/issues"
  },
  "homepage": "https://github.com/codsen/html-img-alt#readme",
  "dependencies": {
    "check-types-mini": "^2.6.0",
    "lodash.isplainobject": "*",
    "string-replace-slices-array": "^1.4.0",
    "string-slices-array-push": "^1.5.2",
    "string-unfancy": "^2.1.0"
  },
  "devDependencies": {
    "ava": "*",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clear-cli": "*",
    "coveralls": "*",
    "eslint": "^4.7.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "husky": "*",
    "nyc": "*"
  },
  "ava": {
    "verbose": true
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
