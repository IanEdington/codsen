{
  "name": "html-img-alt",
  "version": "1.3.2",
  "description": "Adds missing ALT attributes to IMG tags and cleans within IMG tags. No HTML parsing used.",
  "license": "MIT",
  "engines": {
    "node": ">=0.10.0"
  },
  "repository": "codsen/html-img-alt",
  "bugs": {
    "url": "https://github.com/codsen/html-img-alt/issues"
  },
  "homepage": "https://github.com/codsen/html-img-alt#readme",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "main": "index-es5.js",
  "scripts": {
    "build": "babel index.js --presets babel-preset-es2015 --out-file index-es5.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "prepare": "npm run build",
    "test": "clear && ./node_modules/.bin/eslint index.js test.js && nyc --reporter=html --reporter=text ava && npm run build"
  },
  "ava": {
    "verbose": true
  },
  "keywords": [
    "add",
    "email",
    "template",
    "newsletter",
    "marketing",
    "alt",
    "tags",
    "attributes",
    "missing",
    "fill",
    "templates",
    "restore",
    "clean",
    "parsing"
  ],
  "dependencies": {
    "check-types-mini": "^5.1.1",
    "lodash.isplainobject": "^4.0.6",
    "string-replace-slices-array": "^2.2.3",
    "string-slices-array-push": "^2.8.2",
    "string-unfancy": "^3.3.0"
  },
  "devDependencies": {
    "ava": "^1.0.1",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clear-cli": "^1.0.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.3.0",
    "nyc": "^13.1.0"
  }
}
