{
  "name": "html-img-alt",
  "version": "1.2.3",
  "description": "Adds missing ALT attributes to IMG tags and cleans within IMG tags. No HTML parsing used.",
  "main": "index.js",
  "scripts": {
    "build": "babel index.js --presets babel-preset-es2015 --out-dir es5",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "prepare": "npm run build",
    "test": "clear && standard && nyc --reporter=html --reporter=text ava",
    "watch": "nodemon --quiet --watch . --exec npm run test"
  },
  "repository": "codsen/html-img-alt",
  "keywords": [
    "add",
    "email",
    "template",
    "newsletter",
    "marketing",
    "alt",
    "tags",
    "attributes",
    "missing",
    "fill",
    "templates",
    "restore",
    "clean",
    "parsing"
  ],
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/html-img-alt/issues"
  },
  "homepage": "https://github.com/codsen/html-img-alt#readme",
  "dependencies": {
    "check-types-mini": "^2.4.1",
    "lodash.isplainobject": "*",
    "string-replace-slices-array": "^1.3.1",
    "string-slices-array-push": "^1.3.3",
    "string-unfancy": "^2.0.3"
  },
  "devDependencies": {
    "ava": "*",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clear-cli": "*",
    "coveralls": "*",
    "husky": "*",
    "nodemon": "*",
    "nyc": "*",
    "standard": "^10.0.3"
  },
  "ava": {
    "verbose": true
  },
  "engines": {
    "node": ">=5.11.0"
  },
  "standard": {
    "ignore": [
      "es5/"
    ]
  }
}
