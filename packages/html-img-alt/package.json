{
  "name": "html-img-alt",
  "version": "1.0.1",
  "description": "Adds missing ALT attributes to IMG tags and cleans within IMG tags. No HTML parsing used.",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test && doctoc readme.md",
    "test": "clear && standard && nyc --reporter=html --reporter=text ava",
    "watch": "nodemon --quiet --watch . --exec npm run test"
  },
  "repository": "codsen/html-img-alt",
  "keywords": [
    "add",
    "email",
    "template",
    "newsletter",
    "marketing",
    "alt",
    "tags",
    "attributes",
    "missing",
    "fill",
    "templates",
    "restore",
    "clean",
    "parsing"
  ],
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/html-img-alt/issues"
  },
  "homepage": "https://github.com/codsen/html-img-alt#readme",
  "dependencies": {
    "string-replace-slices-array": "^1.0.5",
    "string-slices-array-push": "^1.1.0"
  },
  "devDependencies": {
    "ava": "*",
    "clear-cli": "*",
    "coveralls": "*",
    "husky": "*",
    "nodemon": "*",
    "nyc": "*",
    "standard": "^10.0.2"
  },
  "ava": {
    "verbose": true
  },
  "engines": {
    "node": ">=5.11.0"
  }
}
