{
  "name": "email-all-chars-within-ascii-cli",
  "version": "1.5.0",
  "description": "Command line app to scan email templates, are all their characters within ASCII range",
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/codsen/email-all-chars-within-ascii-cli.git"
  },
  "homepage": "https://bitbucket.org/codsen/email-all-chars-within-ascii-cli/",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "bin": {
    "tinaturner": "cli.js",
    "withinascii": "cli.js"
  },
  "scripts": {
    "coverage": "exit 0",
    "format": "prettier '*.{js,ts,css,less,scss,vue,gql,md}' --write && ./node_modules/.bin/eslint cli.js test/*.js --fix",
    "test": "./node_modules/.bin/eslint cli.js && ava"
  },
  "ava": {
    "compileEnhancements": false,
    "require": [
      "esm"
    ],
    "verbose": true
  },
  "clinton": {
    "rules": {
      "xo": 0
    }
  },
  "esm": {
    "await": true,
    "cjs": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm test"
    }
  },
  "keywords": [
    "email",
    "templates",
    "template",
    "characters",
    "ascii",
    "range",
    "encoded",
    "7bit",
    "validate"
  ],
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "contributors": true,
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "contribution_types": [
      "Blogposts",
      "Bug reports",
      "Code",
      "Design",
      "Documentation",
      "Event Organizing",
      "Examples",
      "Financial",
      "Funding Finding",
      "Ideas, Planning, & Feedback",
      "Infrastructure (Hosting, Build-Tools, etc)",
      "Plugin/utility libraries",
      "Answering Questions",
      "Reviewed Pull Requests",
      "Talks",
      "Tests",
      "Tools",
      "Translation",
      "Tutorials",
      "Videos"
    ],
    "contributors": [
      {
        "contribution": [
          "Code",
          "Documentation",
          "Tests"
        ],
        "username": "revelt"
      }
    ],
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": [
        ""
      ]
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      },
      "devDependencies": [
        "tempy"
      ],
      "travisVersionsOverride": []
    }
  },
  "nyc": {
    "instrument": false,
    "require": [
      "@babel/register"
    ],
    "sourceMap": false
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "email-all-chars-within-ascii": "^2.4.0",
    "fs-extra": "^7.0.0",
    "globby": "^8.0.1",
    "inquirer": "^6.2.0",
    "lodash.pullall": "*",
    "lodash.uniq": "*",
    "meow": "^5.0.0",
    "update-notifier": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "ava": "^1.0.0-beta.7",
    "babel-plugin-istanbul": "^5.1.0",
    "codacy-coverage": "latest",
    "coveralls": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-ava": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-no-unsanitized": "latest",
    "eslint-plugin-no-wildcard-postmessage": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-scanjs-rules": "latest",
    "esm": "latest",
    "execa": "^1.0.0",
    "husky": "latest",
    "nyc": "latest",
    "prettier": "latest",
    "tempy": "latest"
  }
}
