{
  "name": "string-collapse-white-space",
  "version": "1.0.2",
  "description": "Efficient collapsing of white space within strings with optional trimming",
  "main": "es5/index.js",
  "scripts": {
    "build": "babel index.js --presets babel-preset-es2015 --out-dir es5",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "prepublish": "npm run build",
    "test": "standard && nyc --reporter=html --reporter=text ava"
  },
  "repository": "codsen/string-collapse-white-space",
  "keywords": [
    "string",
    "collapse",
    "whitespace",
    "white",
    "space",
    "empty",
    "space",
    "string-utility"
  ],
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/string-collapse-white-space/issues"
  },
  "homepage": "https://github.com/codsen/string-collapse-white-space#readme",
  "dependencies": {
    "check-types-mini": "^2.4.0",
    "lodash.isplainobject": "*",
    "string-replace-slices-array": "^1.3.1",
    "string-slices-array-push": "^1.3.3"
  },
  "devDependencies": {
    "ava": "*",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "*",
    "husky": "*",
    "nyc": "*",
    "standard": "^10.0.3"
  },
  "ava": {
    "verbose": true
  },
  "engines": {
    "node": ">=5.11.0"
  },
  "standard": {
    "ignore": [
      "es5"
    ]
  }
}
