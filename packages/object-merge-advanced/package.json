{
  "name": "object-merge-advanced",
  "version": "10.2.0",
  "description": "Recursive, deep merge of anything (objects, arrays, strings or nested thereof), which weighs contents by type hierarchy to ensure the maximum content is retained",
  "license": "MIT",
  "engines": {
    "node": ">=6",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codsen/object-merge-advanced.git"
  },
  "bugs": {
    "url": "https://github.com/codsen/object-merge-advanced/issues"
  },
  "homepage": "https://github.com/codsen/object-merge-advanced#readme",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "main": "dist/object-merge-advanced.cjs.js",
  "module": "dist/object-merge-advanced.esm.js",
  "browser": "dist/object-merge-advanced.umd.js",
  "scripts": {
    "build": "rollup -c",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "rollup -c --dev --silent",
    "format": "prettier '*.{js,ts,css,less,scss,vue,gql,md}' --write && ./node_modules/.bin/eslint src/*.js test/*.js --fix",
    "precommit": "npm run format && npm test",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/eslint src/*.js test/*.js && nyc --require esm --reporter=html --reporter=text ava"
  },
  "ava": {
    "compileEnhancements": false,
    "require": [
      "esm"
    ],
    "verbose": true
  },
  "clinton": {
    "rules": {
      "xo": 0
    }
  },
  "esm": {
    "await": true,
    "cjs": true
  },
  "keywords": [
    "ast",
    "plain",
    "object",
    "posthtml-tree",
    "posthtmltree",
    "merge",
    "objects",
    "advanced"
  ],
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
