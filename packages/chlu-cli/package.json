{
  "name": "chlu-cli",
  "version": "1.7.1",
  "description": "CH-ange-L-og U-pdate - Automatically fix errors in your changelog file",
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "repository": "codsen/chlu-cli",
  "bugs": {
    "url": "https://github.com/codsen/chlu-cli/issues"
  },
  "homepage": "https://github.com/codsen/chlu-cli#readme",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "bin": {
    "chlu": "cli.js"
  },
  "scripts": {
    "format:pkg": "format-package -w -q",
    "precommit": "npm test && npm run prepare",
    "prepare": "npm run format:pkg",
    "test": "./node_modules/.bin/eslint cli.js"
  },
  "ava": {
    "verbose": true
  },
  "files": [
    "cli.js"
  ],
  "keywords": [
    "update",
    "changelog",
    "change",
    "log",
    "fix",
    "errors",
    "missing",
    "links",
    "markdown",
    "correct",
    "automatically",
    "automated",
    "tool",
    "cli",
    "cli-app"
  ],
  "dependencies": {
    "chlu": "^2.5.1",
    "meow": "^3.7.0",
    "update-notifier": "^2.2.0"
  },
  "devDependencies": {
    "ava": "*",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "execa": "^0.8.0",
    "format-package": "^3.0.1",
    "husky": "*"
  }
}
