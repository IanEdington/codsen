{
  "name": "chlu-cli",
  "version": "1.8.9",
  "description": "CH-ange-L-og U-pdate - Automatically fix errors in your changelog file",
  "license": "MIT",
  "engines": {
    "node": ">=4",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codsen/chlu-cli.git"
  },
  "bugs": {
    "url": "https://github.com/codsen/chlu-cli/issues"
  },
  "homepage": "https://github.com/codsen/chlu-cli#readme",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "bin": {
    "chlu": "cli-es5.js"
  },
  "scripts": {
    "build": "babel cli.js --presets babel-preset-env --out-file cli-es5.js",
    "precommit": "npm test",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/eslint cli.js && npm run build"
  },
  "ava": {
    "verbose": true
  },
  "clinton": {
    "rules": {
      "xo": 0
    }
  },
  "keywords": [
    "update",
    "changelog",
    "change",
    "log",
    "fix",
    "errors",
    "missing",
    "links",
    "markdown",
    "correct",
    "automatically",
    "automated",
    "tool",
    "cli",
    "cli-app"
  ],
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "contributors": true,
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "contribution_types": [
      "Answering Questions",
      "Blogposts",
      "Bug reports*",
      "Code*",
      "Design",
      "Documentation*",
      "Event Organizers",
      "Examples",
      "Financial Support",
      "Funding/Grant Finders",
      "Ideas & Planning",
      "Infrastructure (Hosting, Build-Tools, etc)",
      "Plugin/utility libraries",
      "Reviewed Pull Requests",
      "Talks",
      "Tests*",
      "Tools",
      "Translation",
      "Tutorials",
      "Videos"
    ],
    "contributors": [
      {
        "contribution": [
          "Code",
          "Documentation",
          "Tests"
        ],
        "username": "revelt"
      }
    ],
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": ""
    },
    "licence": {
      "extras": []
    },
    "npmignore": {
      "badFiles": [
        "cli.js"
      ],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      }
    }
  },
  "dependencies": {
    "chlu": "^2.7.3",
    "meow": "^4.0.0",
    "update-notifier": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-ava": "^4.4.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "*"
  }
}
