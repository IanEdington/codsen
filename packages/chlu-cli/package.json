{
  "name": "chlu-cli",
  "version": "1.8.9",
  "description": "CH-ange-L-og U-pdate - Automatically fix errors in your changelog file",
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codsen/chlu-cli.git"
  },
  "bugs": {
    "url": "https://github.com/codsen/chlu-cli/issues"
  },
  "homepage": "https://github.com/codsen/chlu-cli#readme",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "bin": {
    "chlu": "cli.js"
  },
  "scripts": {
    "format": "prettier '*.{js,ts,css,less,scss,vue,gql,md}' --write && ./node_modules/.bin/eslint cli.js test/*.js --fix",
    "precommit": "npm run format && npm test",
    "test": "./node_modules/.bin/eslint cli.js"
  },
  "ava": {
    "verbose": true
  },
  "clinton": {
    "rules": {
      "xo": 0
    }
  },
  "keywords": [
    "update",
    "changelog",
    "change",
    "log",
    "fix",
    "errors",
    "missing",
    "links",
    "markdown",
    "correct",
    "automatically",
    "automated",
    "tool",
    "cli",
    "cli-app"
  ],
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "contributors": true,
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "contribution_types": [
      "Blogposts",
      "Bug reports",
      "Code",
      "Design",
      "Documentation",
      "Event Organizing",
      "Examples",
      "Financial",
      "Funding Finding",
      "Ideas, Planning, & Feedback",
      "Infrastructure (Hosting, Build-Tools, etc)",
      "Plugin/utility libraries",
      "Answering Questions",
      "Reviewed Pull Requests",
      "Talks",
      "Tests",
      "Tools",
      "Translation",
      "Tutorials",
      "Videos"
    ],
    "contributors": [
      {
        "contribution": [
          "Code",
          "Documentation",
          "Tests"
        ],
        "username": "revelt"
      }
    ],
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": []
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      },
      "devDependencies": []
    }
  },
  "dependencies": {
    "chlu": "^2.10.0",
    "meow": "^5.0.0",
    "update-notifier": "^2.5.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-ava": "^4.5.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "*",
    "prettier": "latest"
  }
}
