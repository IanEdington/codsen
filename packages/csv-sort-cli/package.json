{
  "name": "csv-sort-cli",
  "version": "1.9.73",
  "description": "Command line app to sort double-entry CSVs coming from internet banking statements",
  "license": "MIT",
  "repository": "https://gitlab.com/codsen/codsen/",
  "homepage": "https://gitlab.com/codsen/codsen/tree/master/packages/csv-sort-cli",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "keywords": [
    "accountancy",
    "accounting",
    "bank",
    "columns",
    "crunch",
    "csv",
    "file",
    "files",
    "fix",
    "lloyds",
    "missing",
    "patch",
    "pony",
    "rows",
    "sort"
  ],
  "bin": {
    "csvsort": "cli.js",
    "sortcsv": "cli.js"
  },
  "scripts": {
    "ci_test": "tap --no-only --reporter=silent --output-file=testStats.md && npm run format",
    "dev": "echo\"\"",
    "devunittest": "npm run dev && ./node_modules/.bin/tap --only -R 'base'",
    "format": "npm run lect && npm run prettier && npm run lint",
    "lect": "lect",
    "lint": "../../node_modules/eslint/bin/eslint.js \"**/*.js\" --fix --config \"../../.eslintrc.json\" --quiet",
    "prettier": "../../node_modules/prettier/bin-prettier.js '*.{js,css,scss,vue,md}' --write --loglevel silent",
    "republish": "npm publish || :",
    "tap": "tap",
    "test": "npm run lint && npm run unittest && npm run format",
    "unittest": "./node_modules/.bin/tap --no-only --output-file=testStats.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm test"
    }
  },
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "cov": false,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": [
        ""
      ]
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      },
      "devDependencies": [
        "fs-extra"
      ],
      "travisVersionsOverride": []
    }
  },
  "tap": {
    "coverage-report": [
      "json-summary",
      "text"
    ],
    "nyc-arg": [
      "--exclude=**/*.cjs.js",
      "--exclude=**/*.umd.js"
    ],
    "timeout": 0
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "csv-sort": "^3.0.63",
    "globby": "^11.0.1",
    "inquirer": "^7.3.0",
    "lodash.pullall": "^4.2.0",
    "lodash.uniq": "^4.5.0",
    "meow": "^7.0.1",
    "update-notifier": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-strip": "^1.3.3",
    "execa": "^4.0.2",
    "fs-extra": "^9.0.1",
    "lect": "^0.13.6",
    "prettier": "^2.0.5",
    "tap": "^14.10.7",
    "tempy": "^0.5.0"
  }
}
