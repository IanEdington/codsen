    "globby": "^6.1.0",
    "inquirer": "^3.2.2",
    "lodash.pullall": "*",
    "lodash.uniq": "*",
    "meow": "^3.7.0",
    "update-notifier": "^2.2.0"
  },
  "devDependencies": {
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
