{
  "name": "csv-sort-cli",
  "version": "1.9.46",
  "description": "Command line app to sort double-entry CSVs coming from internet banking statements",
  "license": "MIT",
  "engines": {
    "node": ">=8.9"
  },
  "repository": "https://gitlab.com/codsen/codsen/",
  "homepage": "https://gitlab.com/codsen/codsen/tree/master/packages/csv-sort-cli",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "keywords": [
    "csv",
    "sort",
    "fix",
    "bank",
    "accountancy",
    "accounting",
    "lloyds",
    "crunch",
    "patch",
    "pony",
    "rows",
    "columns",
    "missing",
    "file",
    "files"
  ],
  "bin": {
    "csvsort": "cli.js",
    "sortcsv": "cli.js"
  },
  "scripts": {
    "ava": "ava -d",
    "coverage": "nyc report --reporter=json-summary",
    "dev": "echo\"\"",
    "format": "npm run lect && npm run prettier && npm run lint",
    "lect": "lect",
    "lint": "./node_modules/.bin/eslint **/*.js --fix",
    "prettier": "prettier '*.{js,css,scss,vue,md}' --write",
    "test": "npm run lint && npm run unittest && npm run format",
    "unittest": "./node_modules/.bin/ava"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm test"
    }
  },
  "ava": {
    "compileEnhancements": false,
    "require": [
      "esm"
    ],
    "timeout": "99m",
    "verbose": true
  },
  "esm": {
    "await": true,
    "cjs": true
  },
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "cov": false,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": [
        ""
      ]
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      },
      "devDependencies": [
        "fs-extra"
      ],
      "travisVersionsOverride": []
    }
  },
  "nyc": {
    "instrument": false,
    "require": [
      "@babel/register"
    ],
    "sourceMap": false
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "csv-sort": "^3.0.37",
    "globby": "^10.0.1",
    "inquirer": "^7.0.0",
    "lodash.pullall": "^4.2.0",
    "lodash.uniq": "^4.5.0",
    "meow": "^5.0.0",
    "update-notifier": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "ava": "^2.2.6",
    "babel-plugin-istanbul": "^5.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-ava": "^9.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "eslint-plugin-prettier": "^3.1.1",
    "esm": "^3.2.25",
    "execa": "^3.2.0",
    "fs-extra": "^8.1.0",
    "js-row-num-cli": "^1.6.0",
    "lect": "^0.10.1",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "tempy": "^0.3.0"
  }
}
