{
  "name": "csv-sort-cli",
  "version": "1.3.1",
  "description": "Command line app to sort double-entry CSVs coming from internet banking statements",
  "license": "MIT",
  "engines": {
    "node": ">=4",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codsen/csv-sort-cli.git"
  },
  "bugs": {
    "url": "https://github.com/codsen/csv-sort-cli/issues"
  },
  "homepage": "https://github.com/codsen/csv-sort-cli#readme",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "bin": {
    "csvsort": "cli-es5.js",
    "sortcsv": "cli-es5.js"
  },
  "scripts": {
    "build": "babel cli.js --presets babel-preset-env --out-file cli-es5.js",
    "precommit": "npm test",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/eslint cli.js && npm run build"
  },
  "ava": {
    "verbose": true
  },
  "clinton": {
    "rules": {
      "xo": 0
    }
  },
  "files": [
    "cli-es5.js"
  ],
  "keywords": [
    "csv",
    "sort",
    "fix",
    "bank",
    "accountancy",
    "accounting",
    "lloyds",
    "crunch",
    "patch",
    "pony",
    "rows",
    "columns",
    "missing",
    "file",
    "files"
  ],
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "contributors": true,
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "contribution_types": [
      "Answering Questions",
      "Blogposts",
      "Bug reports*",
      "Code*",
      "Documentation*",
      "Design",
      "Examples",
      "Event Organizers",
      "Financial Support",
      "Funding/Grant Finders",
      "Ideas & Planning",
      "Infrastructure (Hosting, Build-Tools, etc)",
      "Plugin/utility libraries",
      "Reviewed Pull Requests",
      "Tools",
      "Translation",
      "Tests*",
      "Tutorials",
      "Talks",
      "Videos"
    ],
    "contributors": [
      {
        "contribution": [
          "Code",
          "Documentation",
          "Tests"
        ],
        "username": "revelt"
      }
    ],
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": ""
    },
    "licence": {
      "extras": []
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      }
    }
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "csv-sort": "^2.0.3",
    "globby": "^7.1.1",
    "inquirer": "^4.0.1",
    "lodash.pullall": "*",
    "lodash.uniq": "*",
    "meow": "^4.0.0",
    "update-notifier": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-ava": "^4.4.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "*"
  }
}
