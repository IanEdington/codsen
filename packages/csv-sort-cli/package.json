{
  "name": "csv-sort-cli",
  "version": "1.7.1",
  "description": "Command line app to sort double-entry CSVs coming from internet banking statements",
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "repository": "https://bitbucket.org/codsen/codsen/src/master/packages/csv-sort-cli",
  "homepage": "https://bitbucket.org/codsen/codsen/src/master/packages/csv-sort-cli",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "bin": {
    "csvsort": "cli.js",
    "sortcsv": "cli.js"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "format": "prettier '*.{js,ts,css,less,scss,vue,gql,md}' --write && ./node_modules/.bin/eslint cli.js test/*.js --fix",
    "precommit": "npm run format && npm test",
    "test": "./node_modules/.bin/eslint cli.js && nyc --reporter=html --reporter=text ava"
  },
  "ava": {
    "compileEnhancements": false,
    "require": [
      "esm"
    ],
    "verbose": true
  },
  "clinton": {
    "rules": {
      "xo": 0
    }
  },
  "esm": {
    "await": true,
    "cjs": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm test"
    }
  },
  "keywords": [
    "csv",
    "sort",
    "fix",
    "bank",
    "accountancy",
    "accounting",
    "lloyds",
    "crunch",
    "patch",
    "pony",
    "rows",
    "columns",
    "missing",
    "file",
    "files"
  ],
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "contributors": true,
      "cov": false,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "contribution_types": [
      "Blogposts",
      "Bug reports",
      "Code",
      "Design",
      "Documentation",
      "Event Organizing",
      "Examples",
      "Financial",
      "Funding Finding",
      "Ideas, Planning, & Feedback",
      "Infrastructure (Hosting, Build-Tools, etc)",
      "Plugin/utility libraries",
      "Answering Questions",
      "Reviewed Pull Requests",
      "Talks",
      "Tests",
      "Tools",
      "Translation",
      "Tutorials",
      "Videos"
    ],
    "contributors": [
      {
        "contribution": [
          "Code",
          "Documentation",
          "Tests"
        ],
        "username": "revelt"
      }
    ],
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": []
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      },
      "devDependencies": [],
      "travisVersionsOverride": []
    }
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "csv-sort": "^2.4.1",
    "globby": "^8.0.1",
    "inquirer": "^6.2.1",
    "lodash.pullall": "^4.2.0",
    "lodash.uniq": "^4.5.0",
    "meow": "^5.0.0",
    "update-notifier": "^2.5.0"
  },
  "devDependencies": {
    "ava": "^1.0.1",
    "codacy-coverage": "^3.3.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.11.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-ava": "^5.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "eslint-plugin-no-wildcard-postmessage": "^0.1.3",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "esm": "^3.0.84",
    "execa": "^1.0.0",
    "fs-extra": "^7.0.1",
    "husky": "^1.2.1",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3",
    "tempy": "^0.2.1"
  }
}
