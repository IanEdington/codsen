{
  "name": "string-remove-thousand-separators",
  "version": "3.0.63",
  "description": "Detects and removes thousand separators (dot/comma/quote/space) from string-type digits",
  "license": "MIT",
  "repository": "https://gitlab.com/codsen/codsen/",
  "homepage": "https://gitlab.com/codsen/codsen/tree/master/packages/string-remove-thousand-separators",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "keywords": [
    "amount",
    "currency",
    "digits",
    "from",
    "numbers",
    "remove",
    "separator",
    "separators",
    "string",
    "thousand",
    "thousands"
  ],
  "main": "dist/string-remove-thousand-separators.cjs.js",
  "module": "dist/string-remove-thousand-separators.esm.js",
  "browser": "dist/string-remove-thousand-separators.umd.js",
  "scripts": {
    "build": "rollup -c",
    "ci_test": "npm run build && npm run lint && tap --no-only --reporter=silent --output-file=testStats.md && npm run format",
