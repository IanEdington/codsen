{
  "name": "string-remove-thousand-separators",
  "version": "2.0.1",
  "description": "Detects and removes thousand separators (dot/comma/quote/space) from string-type digits",
  "license": "MIT",
  "engines": {
    "node": ">=6",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codsen/string-remove-thousand-separators.git"
  },
  "bugs": {
    "url": "https://github.com/codsen/string-remove-thousand-separators/issues"
  },
  "homepage": "https://github.com/codsen/string-remove-thousand-separators#readme",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "main": "dist/string-remove-thousand-separators.cjs.js",
  "module": "dist/string-remove-thousand-separators.esm.js",
  "browser": "dist/string-remove-thousand-separators.umd.js",
  "scripts": {
    "build": "rollup -c",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "rollup -c -w",
    "precommit": "npm test",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/eslint src/*.js test/*.js && nyc --reporter=html --reporter=text ava"
  },
  "ava": {
    "verbose": true
  },
  "clinton": {
    "rules": {
      "xo": 0
    }
  },
  "keywords": [
    "string",
    "remove",
    "thousand",
    "thousands",
    "separator",
    "separators",
    "numbers",
    "digits",
    "currency",
    "amount",
    "from"
  ],
  "dependencies": {
    "check-types-mini": "^2.7.0",
    "is-numeric": "0.0.5",
    "lodash.isplainobject": "*",
    "lodash.trim": "*",
    "string-replace-slices-array": "^2.0.1",
    "string-slices-array-push": "^1.6.0"
  },
  "devDependencies": {
    "ava": "*",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "*",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-ava": "^4.2.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "*",
    "nyc": "*",
    "rollup": "^0.51.1",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "uglify-es": "^3.1.8"
  }
}
