{
  "name": "str-indexes-of-plus",
  "version": "1.0.1",
  "description": "Search for a string in another string. Get array of indexes. Full Unicode support.",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test && doctoc readme.md",
    "test": "standard && nyc --reporter=html --reporter=text ava"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/code-and-send/str-indexes-of-plus.git"
  },
  "keywords": [
    "string",
    "indexes",
    "array",
    "search",
    "plus",
    "find",
    "in",
    "unicode",
    "emoji",
    "astral"
  ],
  "author": "Roy Reveltas <roy@codeandsend.com> (http://codeandsend.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/code-and-send/str-indexes-of-plus/issues"
  },
  "homepage": "https://github.com/code-and-send/str-indexes-of-plus#readme",
  "dependencies": {
    "is-natural-number": "*",
    "is-natural-number-string": "*",
    "lodash.toarray": "*"
  },
  "devDependencies": {
    "ava": "*",
    "coveralls": "*",
    "husky": "*",
    "nyc": "*",
    "standard": "^9.0.2"
  },
  "ava": {
    "verbose": true
  },
  "engines": {
    "node": ">=5.11.0"
  }
}
