{
  "name": "html-table-patcher",
  "version": "1.1.29",
  "description": "Wraps any content between TR/TD tags in additional rows/columns to appear in browser correctly",
  "license": "MIT",
  "engines": {
    "node": ">=8.9"
  },
  "repository": "https://gitlab.com/codsen/codsen/",
  "homepage": "https://gitlab.com/codsen/codsen/tree/master/packages/html-table-patcher",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "keywords": [
    "add",
    "email",
    "template",
    "newsletter",
    "marketing",
    "alt",
    "tags",
    "attributes",
    "missing",
    "fill",
    "templates",
    "restore",
    "clean",
    "parsing"
  ],
  "main": "dist/html-table-patcher.cjs.js",
  "module": "dist/html-table-patcher.esm.js",
  "browser": "dist/html-table-patcher.umd.js",
  "scripts": {
    "ava": "ava -d",
    "build": "rollup -c",
    "coverage": "nyc report --reporter=json-summary",
    "dev": "rollup -c --dev --silent",
    "format": "npm run lect && npm run prettier && npm run lint",
    "lect": "lect",
    "lint": "./node_modules/.bin/eslint \"**/*.js\" --fix --ignore-pattern \"dist/*\" --ignore-pattern \"rollup.config.js\" --ignore-pattern \"tap/**\"",
    "perf": "node perf/check",
    "prepare": "npm run build",
    "prettier": "prettier '*.{js,css,scss,vue,md}' --write",
    "pretest": "npm run build",
    "test": "npm run lint && npm run unittest && npm run format",
    "unittest": "./node_modules/.bin/nyc ava && npm run coverage && npm run perf",
    "version": "npm run build && git add ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm test"
    }
  },
  "ava": {
    "compileEnhancements": false,
    "require": [
      "esm"
    ],
    "timeout": "99m",
    "verbose": true
  },
  "esm": {
    "await": true,
    "cjs": true
  },
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "eslintrc": {
      "add": [],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": [
        ""
      ]
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "req": "{ patcher, defaults, version }",
    "various": {
      "devDependencies": [
        "html-crush",
        "object-delete-key",
        "rollup-plugin-node-builtins",
        "rollup-plugin-node-globals",
        "json-stringify-safe"
      ],
      "travisVersionsOverride": []
    }
  },
  "nyc": {
    "instrument": false,
    "require": [
      "@babel/register"
    ],
    "sourceMap": false
  },
  "dependencies": {
    "dom-serializer": "^0.2.1",
    "domutils-bastardised": "^1.7.1",
    "html-comment-regex": "^1.1.2",
    "html-dom-parser": "^0.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "ava": "^2.2.7",
    "babel-plugin-istanbul": "^5.2.0",
    "benchmark": "^2.1.4",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-ava": "^9.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "eslint-plugin-prettier": "^3.1.1",
    "esm": "^3.2.25",
    "html-crush": "^1.9.9",
    "js-row-num-cli": "^1.6.3",
    "json-stringify-safe": "^5.0.1",
    "lect": "^0.10.5",
    "nyc": "^14.1.1",
    "object-delete-key": "^1.9.5",
    "prettier": "^1.18.2",
    "rollup": "^1.26.3",
    "rollup-plugin-ascii": "^0.0.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-strip": "^1.2.2",
    "rollup-plugin-terser": "^5.1.2"
  }
}
