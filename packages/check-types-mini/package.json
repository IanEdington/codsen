{
  "name": "check-types-mini",
  "version": "2.7.0",
  "description": "Check the types of your options object's values after user has customised them",
  "license": "MIT",
  "engines": {
    "node": ">=6",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "repository": "codsen/check-types-mini",
  "bugs": {
    "url": "https://github.com/codsen/check-types-mini/issues"
  },
  "homepage": "https://github.com/codsen/check-types-mini#readme",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "main": "index-es5.js",
  "scripts": {
    "build": "babel index.js --presets babel-preset-env --out-file index-es5.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "prepare": "npm run build",
    "test": "clear && ./node_modules/.bin/eslint index.js test.js && npm run build && nyc --reporter=html --reporter=text ava",
    "watch": "nodemon --quiet --watch . --exec npm run test"
  },
  "ava": {
    "verbose": true
  },
  "keywords": [
    "plain",
    "object",
    "deep",
    "keys",
    "unique",
    "no",
    "difference",
    "compare",
    "new",
    "match",
    "schema",
    "validate"
  ],
  "dependencies": {
    "arrayiffy-if-string": "*",
    "lodash.includes": "*",
    "lodash.intersection": "*",
    "lodash.pullall": "*",
    "type-detect": "^4.0.3"
  },
  "devDependencies": {
    "ava": "*",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "clear-cli": "*",
    "coveralls": "*",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-ava": "^4.2.2",
    "eslint-plugin-import": "^2.7.0",
    "husky": "*",
    "nodemon": "*",
    "nyc": "*"
  }
}
