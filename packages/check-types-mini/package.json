{
  "name": "check-types-mini",
  "version": "1.3.0",
  "description": "Check the types of your options object's values after user has customised them",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test && doctoc readme.md",
    "test": "clear && standard && nyc --reporter=html --reporter=text ava",
    "watch": "nodemon --quiet --watch . --exec npm run test"
  },
  "repository": "codsen/check-types-mini",
  "keywords": [
    "plain",
    "object",
    "deep",
    "keys",
    "unique",
    "no",
    "difference",
    "compare",
    "new",
    "match",
    "schema",
    "validate"
  ],
  "author": {
    "name": "Roy Reveltas",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/check-types-mini/issues"
  },
  "homepage": "https://github.com/codsen/check-types-mini#readme",
  "dependencies": {
    "arrayiffy-if-string": "*",
    "lodash.clonedeep": "^4.5.0",
    "lodash.includes": "*",
    "lodash.pullall": "^4.2.0",
    "object-assign": "^4.1.1",
    "type-detect": "^4.0.0"
  },
  "devDependencies": {
    "ava": "*",
    "clear-cli": "*",
    "coveralls": "*",
    "husky": "^0.13.4",
    "nodemon": "*",
    "nyc": "^11.0.2",
    "standard": "^10.0.0"
  },
  "ava": {
    "verbose": true
  },
  "engines": {
    "node": ">=5.11.0"
  }
}
