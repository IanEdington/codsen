{
  "name": "check-types-mini",
  "version": "2.4.0",
  "description": "Check the types of your options object's values after user has customised them",
  "main": "index.js",
  "scripts": {
    "build": "babel index.js --presets babel-preset-es2015 --out-dir es5",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "prepare": "npm run build",
    "test": "clear && standard && nyc --reporter=html --reporter=text ava",
    "watch": "nodemon --quiet --watch . --exec npm run test"
  },
  "repository": "codsen/check-types-mini",
  "keywords": [
    "plain",
    "object",
    "deep",
    "keys",
    "unique",
    "no",
    "difference",
    "compare",
    "new",
    "match",
    "schema",
    "validate"
  ],
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/check-types-mini/issues"
  },
  "homepage": "https://github.com/codsen/check-types-mini#readme",
  "dependencies": {
    "arrayiffy-if-string": "*",
    "lodash.includes": "*",
    "lodash.intersection": "*",
    "lodash.pullall": "*",
    "type-detect": "^4.0.0"
  },
  "devDependencies": {
    "ava": "*",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clear-cli": "*",
    "coveralls": "*",
    "husky": "*",
    "nodemon": "*",
    "nyc": "^11.0.2",
    "standard": "^10.0.0"
  },
  "ava": {
    "verbose": false
  },
  "engines": {
    "node": ">=5.11.0"
  },
  "standard": {
    "ignore": [
      "es5/"
    ]
  }
}
